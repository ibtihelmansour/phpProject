(function(){function wrapCallback(cb){return function(){try{cb.apply(this,arguments)}catch(e){console.log("ERROR: "+(e.stack?e.stack:e))}return!0}}var HelpClient=HelpClient||{};Mochat.HelpClient=HelpClient;var noop=Nomasystems.ChatCore.Util.noop,deepFreeze=Nomasystems.ChatCore.Util.deepFreeze,client=null,onMessage=noop,onResourceLogout=noop,onReceipt=noop,onClose=noop,onDisconnect=noop,onTypingOn=noop,onTypingOff=noop,onReconnecting=noop,onReconnected=noop,currentWorkgroupJid=null;HelpClient.helpClientCallbackEnum=
deepFreeze({OnInvite:1,OnMessage:2,OnReceipt:3,OnClose:4,OnDisconnect:5,OnTypingOn:6,OnTypingOff:7,OnReconnecting:8,OnReconnected:9});HelpClient.init=function(URL){client||(client=new Nomasystems.ChatCore.Client(URL),client.addHandler(Nomasystems.ChatCore.Client.EventEnum.OnWorkgroupMessage,function(message){var mochatMessage=Mochat.Datatypes.Message.parseChatCoreMessage(message,client.getConnectedJid());switch(message.state){case Nomasystems.ChatCore.Model.Message.State.Composing:onTypingOn();break;
