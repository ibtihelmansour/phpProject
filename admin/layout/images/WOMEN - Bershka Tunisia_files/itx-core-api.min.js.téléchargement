bg_offset;$("article").each(function(){section_top=$(this).offset().top;var current_scroll=window.scrollY||document.documentElement.scrollTop;bg_offset=current_scroll-section_top;0<bg_offset?($(this).css("background-position","50% "+(bg_offset+100)+"px"),$(this).css("background-position-y",bg_offset+100+"px"),$(this).css("-ms-background-position-y",bg_offset+100+"px")):($(this).css("background-position","50% 100px"),$(this).css("background-position-y","100px"),$(this).css("-ms-background-position-y",
"100px"))})};$(window).scroll(function(){position_bg();var footerVisible=bershka.isFooterVisible();footerVisible!=self.iScope.footerVisible&&(self.iScope.footerVisible=footerVisible,self.iScope.$apply())});$(".showmore").bind("click",function(){var article=$(this).closest("article");if(article.hasClass("collapsed")){var etiqueta="";article.hasClass("return")&&(etiqueta="envios_y_devoluciones");article.hasClass("products")&&(etiqueta="productos");article.hasClass("payment")&&(etiqueta="metodos_pago");
