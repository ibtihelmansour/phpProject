break}break}for(var sizeFound=!1,j=0;j<sizes.length;j++)if(sizes[j].width===width&&sizes[j].height===height){codeSize=sizes[j].code;sizeFound=!0;break}if(!sizeFound&&0<sizes.length)for(sizes.push({width:width,height:height}),sizes.sort(function(a,b){a=[a.width,a.height];b=[b.width,b.height];return a[0]>=b[0]&&a[1]>=b[1]?-1:1}),j=0;j<sizes.length;j++){var prevSize=sizes[0===j?0:j-1];if(sizes[j].width===width&&sizes[j].height===height){codeSize=prevSize.code?prevSize.code:sizes[j+1].code;break}}if(""!==
