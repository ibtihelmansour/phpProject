productDetailCtrl.prototype._loadProduct=function(){function processActions(results){var product=results[1],stock=results[2],nextProduct=results[3],previousProduct=results[4];product.state==self.executeCache.STATE_REJECTED||stock.state==self.executeCache.STATE_REJECTED?(self._setLoading(!1),inditex.xOpenErrorPopUp(278,"auto")):(product=product.value,stock=stock.value,self.iColor=null,self.oNextProductDescriptor?nextProduct.state==self.executeCache.STATE_RESOLVED&&self._setNavigableProduct(nextProduct.value,
self.oNextProductDescriptor):previousProduct=nextProduct,self.oPreviousProductDescriptor&&previousProduct.state==self.executeCache.STATE_RESOLVED&&self._setNavigableProduct(previousProduct.value,self.oPreviousProductDescriptor),self.categoryFactory.oCategory||self.$timeout(function(){var cat=self.stateFactory.search("categoryId");cat&&self.categoryFactory.setCategoryId(cat)}),product.selectedColorId=self.paramColorId,product.selectedStyleId=self.paramStylismId,bershka.scrollToTop("instant"),self.productNormalizerService.normalize([product],
stock,!0).done(function(prod){self._prepareProduct(prod[0])}))}var self=this;this._setLoading(!0);bershka.scrollToTop("instant");this.paramColorId=this.stateFactory.search("colorId");this.paramStylismId=this.stateFactory.search("stylismId");var actions=[{action:"user:currentUser",params:{storeId:inditex.iStoreId}},{action:"catalog:productDetail",params:{storeId:inditex.iStoreId,languageId:inditex.iLangId,catalogId:inditex.iCatalogId,categoryId:this.categoryFactory.oCategory?this.categoryFactory.oCategory.id:
"0",productId:inditex.iProductId}},{action:"catalog:productStock",params:{storeId:inditex.iStoreId,languageId:inditex.iLangId,catalogId:inditex.iCatalogId,productId:inditex.iProductId}}];this.oNextProductDescriptor&&actions.push({action:"catalog:productDetail",params:{storeId:inditex.iStoreId,languageId:inditex.iLangId,catalogId:inditex.iCatalogId,categoryId:this.oNextProductDescriptor.descriptor.categoryId,productId:this.oNextProductDescriptor.descriptor.bundleId}});this.oPreviousProductDescriptor&&
actions.push({action:"catalog:productDetail",params:{storeId:inditex.iStoreId,languageId:inditex.iLangId,catalogId:inditex.iCatalogId,categoryId:this.oPreviousProductDescriptor.descriptor.categoryId,productId:this.oPreviousProductDescriptor.descriptor.bundleId}});this.bskSeo.loadProductId(inditex.iProductId,null);this.executeCache.execute(actions).then(processActions,processActions)};
productDetailCtrl.prototype._setNavigableProduct=function(detail,descriptor){detail.selectedColorId=descriptor.descriptor.colorId;detail.selectedStyleId=descriptor.descriptor.styleId;detail.bundleId=descriptor.descriptor.bundleId;this.productNormalizerService.normalize([detail],[],!0).done(function(prod){var normalized=prod[0];if(normalized){descriptor.product=normalized;descriptor.color=normalized.colors[0];descriptor.price=normalized.price;for(var images=normalized.image.PRODUCT_PAGE.SMALL,i=0,
len=images.length;i<len;i++){var image=images[i];if(!image.video){descriptor.image=image.src;break}}descriptor.url=normalized.colors.length?normalized.colors[0].url:normalized.url}})};
productDetailCtrl.prototype.colorSelected=function(color){var product=this.iProduct;this.iColor=color;this.triggerRelatedProducts=!0;this._setDetailImages();var desc=inditex.capitalize(inditex.iBrand)+" "+inditex.capitalize(inditex.iCountryName)+" - "+product.name,socialLinks=inditex.getSocialLinks(inditex.iPage,product.id,desc,color?color.id:null,this.aDetailImages[0]||null);this.iSocialLinks=[];for(var key in socialLinks)"weibo"==key&&"CN"!=inditex.iCountryCode||this.iSocialLinks.push({key:key,
url:socialLinks[key]});this.trackingService.setCategoryPath(inditex.iCategoryId);color=color||{};this.trackingService.trackPageview({name:"page-detalle-producto",data:{product:product,modelName:color.modelName,color:color,list:product.originalProduct.detail.reference,nextProduct:this.oNextProductDescriptor?this.oNextProductDescriptor.product:void 0,previousProduct:this.oPreviousProductDescriptor?this.oPreviousProductDescriptor.product:void 0,keyword:this.searchConversion?this.searchConversion.query:
void 0}});this._setLoading(!1)};
productDetailCtrl.prototype._bindEvents=function(){var self=this;this.iIsProductBean&&($(document).off(bershka.events.productColorSelected+".product-detail"),$(document).on(bershka.events.productColorSelected+".product-detail",function(ev,data){var color=data.color;if(self.stateFactory.search("colorId")!=color.id){if(self.productNormalizerService.copyDataFromBundle){var activeMocaco=self.productHelpers.getActiveMocaco(self.iProduct.id,color.id,self.gridOrchestrator.getProducts());activeMocaco&&(self.iProduct=
activeMocaco,color=activeMocaco.colors[0])}self.stateFactory.url(color.url).replace()}self.colorSelected(color)}));$(document).off(bershka.events.AddToCart+".product-detail");$(document).on(bershka.events.AddToCart+".product-detail",function(ev,data){1==data.status&&self.searchConversion&&self.searchServiceAPI.trackAdd2Cart(self.searchConversion.product,self.searchConversion.query,self.searchConversion.page)})};
productDetailCtrl.prototype._getAutoSearchQuery=function(uri){for(var name=decodeURIComponent(uri).replace(/-/g," ").split("/").pop(),splitted=name.split(" "),i=splitted.length;i--;)splitted[i].length<this.autosearch.minlength&&splitted.splice(i,1);return-1==this.autosearch.maxwords?splitted.join(" "):splitted.splice(0,this.autosearch.maxwords).join(" ")};
productDetailCtrl.prototype._prepareProduct=function(product){if(!product||"ProductBean"==product.type&&product.colors&&0==product.colors.length){var name;product&&product.name?name=product.name:inditex.iParams.keyWord&&(name=this._getAutoSearchQuery(inditex.iParams.keyWord));name&&this.navigationFactory.setAutoSearchTerm(name)}else{this.iIsProductBean="ProductBean"==product.type;this.iProduct=product;this.uniqueProducts.add(product);0==inditex.iCategoryId&&product.relatedCategories.length&&(inditex.iCategoryId=
product.relatedCategories[0].id);this.lastViewedFactory.add(product,this.paramColorId,this.paramStyleId);this._bindEvents();this.iIsProductBean?this.productElements=[product]:(this.colorSelected(product.bundleColors&&product.bundleColors.length?product.bundleColors[0].id:null),this.productElements=product.products);this.hashTags=inditex.getHashTags(product);this.isTriman=!1;if(product.attributes&&-1!=this.trimanCountries.indexOf(inditex.iCountryCode))for(var i=0,len=product.attributes.length;i<len;i++)if("TRIMAN"==
product.attributes[i].name){this.isTriman=!0;break}this.oPreviousProductDescriptor&&!this.oPreviousProductDescriptor.product&&(this.oPreviousProductDescriptor=null);this.oNextProductDescriptor&&!this.oNextProductDescriptor.product&&(this.oNextProductDescriptor=null)}};productDetailCtrl.prototype._setIsCommingFromSearch=function(){this.searchConversion=inditex.getSessionStorage("LAST_SEARCH_CONVERSION");inditex.removeSessionStorage("LAST_SEARCH_CONVERSION")};
productDetailCtrl.prototype._prepareNavigation=function(){function check(method){for(var i=0,len=internalIDs.length;i<len;i++){var currentInternal=internalIDs[i],descriptor=self.gridRepository[method](function(id,value){for(var key in currentInternal)if(value[key]!=currentInternal[key])return!1;return!0});if(descriptor)return{descriptor:descriptor}}return null}var self=this,colorId=this.stateFactory.search("colorId"),product=this.iProduct;!colorId&&product&&product.detail&&product.detail.colors&&
product.detail.colors.length&&(colorId=product.detail.colors[0].id);var styleId=this.stateFactory.search("stylismId"),productId=inditex.iProductId+"",internalIDs=[{bundleId:productId,colorId:colorId,categoryId:inditex.iCategoryId,styleId:styleId},{bundleId:productId,colorId:colorId,categoryId:"0",styleId:styleId},{bundleId:productId,colorId:colorId,styleId:styleId},{bundleId:productId,colorId:colorId},{bundleId:productId}];this.oNextProductDescriptor=check("nextFor");this.oPreviousProductDescriptor=
check("prevFor")};productDetailCtrl.prototype.showVideo=function(){this.iCurrentVideo=this.aDetailVideos[this.iActiveVideo];"mobile"!=this.deviceService.getActualState()&&bershka.hideScrolls()};productDetailCtrl.prototype.hideVideo=function(){this.iCurrentVideo&&bershka.showScrolls();this.iCurrentVideo=null};productDetailCtrl.prototype.sliderSlide=function(element,newIndex){var self=this;this.$timeout(function(){self.iActiveImage=newIndex})};
productDetailCtrl.prototype.goBackToCategory=function($event){$event&&($event.preventDefault(),$event.stopPropagation());this.navigationFactory.setCategory(this.categoryFactory.oCategory)};productDetailCtrl.prototype.goBackToSearch=function(){this.searchConversion?inditex.xGoToSearchPage(this.searchConversion.query):history.back()};
productDetailCtrl.prototype.canShowBackToCategory=function(){var category=this.categoryFactory.oCategory;if(!category||this.searchConversion)return!1;var isMobile=inditex.xDesktopMobileScreenModule.isMobileScreen(),visibility=isMobile?category._config.showMobile:category._config.showDesktop;if(visibility)return visibility;var linkedHere=this.categoryFactory.findCategoryByProperty("viewCategoryId",category.id).pop();return linkedHere?isMobile?linkedHere._config.showMobile:linkedHere._config.showDesktop:
!1};productDetailCtrl.prototype.goToPreviousProduct=function($event){$event&&$event.preventDefault();this._goToDescriptor(this.oPreviousProductDescriptor)};productDetailCtrl.prototype.goToNextProduct=function($event){$event&&$event.preventDefault();this._goToDescriptor(this.oNextProductDescriptor)};productDetailCtrl.prototype.setActiveImage=function(index,$event){$event&&$event.preventDefault();this.iActiveImage=index;this.hideVideo()};
productDetailCtrl.prototype.setPreviousActiveImage=function($event,cycle){$event&&($event.stopPropagation(),$event.preventDefault());var len=this.aDetailImages.length;0==this.iActiveImage?cycle&&this.setActiveImage(len-1):this.setActiveImage(this.iActiveImage-1)};
productDetailCtrl.prototype.setNextActiveImage=function($event,cycle){$event&&($event.stopPropagation(),$event.preventDefault());var len=this.aDetailImages.length;this.iActiveImage==len-1?cycle&&this.setActiveImage(0):this.setActiveImage(this.iActiveImage+1)};
productDetailCtrl.prototype.toggleZoomVisibility=function($event){$event&&($event.stopPropagation(),$event.preventDefault());if(!inditex.xDesktopMobileScreenModule.isMobileScreen()){this.bShowZoomImage=!this.bShowZoomImage;var self=this;this.bShowZoomImage?(bershka.hideScrolls(),this.$timeout(function(){$("#superZoomContainer").imagesLoaded(function(){self.moveZoomImage($event);self.showMouseTip()})},0,!1)):(bershka.showScrolls(),this.hideMouseTip())}};productDetailCtrl.prototype.showMouseTip=function(){$("#mouseTip").show()};
productDetailCtrl.prototype.hideMouseTip=function(){$("#mouseTip").hide()};productDetailCtrl.prototype.moveZoomImage=function($event){var $photo=$("#superZoomContainer img"),$window=$(window),posY=($window.height()-$photo.height())*($event.pageY-bershka.scrollTop())/$window.height();$photo.css("top",posY+"px");$("#mouseTip").css({left:$event.pageX+10,top:$event.pageY+10})};
productDetailCtrl.prototype.openShareUrl=function(link){var url=link.url;"pinterest"==link.key&&this.aDetailImages.length&&(url+="\x26media\x3d"+this.aDetailImages[0]);window.open(url,"_blank","height\x3d500,width\x3d600,status\x3d1,toolbar\x3d0,menubar\x3d0,location\x3d0,resizable\x3d1,scrollbars\x3d1")};
productDetailCtrl.prototype._goToDescriptor=function(aProduct){if(aProduct){this.filterRepository.updatePersistence();this.gridRepository.updatePersistence();this._setSearchStorage(aProduct.descriptor);inditex.setSessionStorage("initialImage",aProduct.image);var pos=this.gridOrchestrator.getProductOriginalIndexByProperty("_trackId",aProduct.product._trackId)+1;inditex.setSessionStorage("productPosition",pos);this.navigationFactory.setProductId(aProduct.url,aProduct.product.bundleId||aProduct.product.id,
aProduct.color&&aProduct.color.id,aProduct.descriptor.styleId,aProduct.descriptor.categoryId);this.trackingService.trackEvent({name:"product-click",data:{product:aProduct.product,newProduct:aProduct.product,list:"parrilla",position:pos,selectedColor:aProduct.product.colors[0],cf:"catalogo",eventCategory:"parrilla",action:"ver_producto",imagen:aProduct.image}})}};
productDetailCtrl.prototype._setSearchStorage=function(descriptor){if(this.searchConversion&&descriptor){var position=this.gridRepository.indexOf(function(index,cmp){for(var key in descriptor)if(descriptor[key]!=cmp[key])return!1;return!0});if(-1!=position){var page=position/bershka.config("SearchPageSize");this.searchConversion.page=Math.floor(page)+1}this.searchConversion.product=descriptor;inditex.setSessionStorage("LAST_SEARCH_CONVERSION",this.searchConversion)}};
productDetailCtrl.prototype._setDetailImages=function(){function getByVideoProp(elements,value){for(var ret=[],i=0,len=elements.length;i<len;i++){var element=elements[i];element.video==value&&ret.push(element)}return ret}var color=this.iIsProductBean?this.iColor:this.iProduct;if(color){var aDetailImages=color.image.PRODUCT_PAGE.MEDIUM,aThumbnailImages=color.image.PRODUCT_PAGE.SMALL,aZoomImages=color.image.PRODUCT_PAGE.LARGE;this.aDetailVideos=getByVideoProp(aDetailImages,!0);this.aDetailImages=getByVideoProp(aDetailImages,
!1);this.aThumbnailImages=getByVideoProp(aThumbnailImages,!1);this.aZoomImages=getByVideoProp(aZoomImages,!1)}};
angular.module("itxApp").component("productDetailElement",{bindings:{iProduct:"\x3cproduct",parentType:"@",iStock:"\x3cstock"},templateUrl:"tplProductDetailElement",controller:["$attrs","$element","$timeout","bskNavigationFactory","shopCartService","trackingService","ItxExecuteCacheProxy","RTBHouseService","bskImageService","jetloreTracking","bskAttributesHelper","bskConfigurationFactory","bskCategoryFactory","bskProductHelpers","gridOrchestrator",productDetailElementCtrl]});
function productDetailElementCtrl($attrs,$element,$timeout,navigationFactory,shopCartService,trackingService,executeCacheProxy,RTBHouseService,imageService,jetloreTracking,attributesHelper,config,categoryFactory,productHelpers,gridOrchestrator){this.$element=$element;this.$timeout=$timeout;this.imageService=imageService;this.core=inditex;this.shopCartService=shopCartService;this.trackingService=trackingService;this.executeCacheProxy=executeCacheProxy;this.RTBHouseService=RTBHouseService;this.jetloreTracking=
jetloreTracking;this.attributesHelper=attributesHelper;this.productHelpers=productHelpers;this.config=config;this.categoryFactory=categoryFactory;this.navigationFactory=navigationFactory;this.gridOrchestrator=gridOrchestrator;this.parseOptions($attrs);this.init.apply(this,arguments)}productDetailElementCtrl.prototype.init=function(){};
productDetailElementCtrl.prototype.defaultOptions={showName:!0,showDescription:!0,showLabels:!0,showReference:!0,showModelInfo:!0,showPrice:!0,showColors:!0,showSizes:!0,showSizeGuide:!0,showLongDescription:!0,showMspots:!0,showAddCart:!0,showAddWishlist:!0,showSearchStore:!0,showCompositionInfo:!0,showDeliveryInfo:!0,showQuantity:!1};
productDetailElementCtrl.prototype.parseOptions=function($attrs){for(var key in this.defaultOptions)$attrs.hasOwnProperty(key)?this[key]="1"==$attrs[key]||"true"==$attrs[key]:this[key]=this.defaultOptions[key]};
productDetailElementCtrl.prototype.$onInit=function(){this.hashTags=inditex.getHashTags(this.iProduct);this.isOpenForSale=inditex.isOpenForSale();this.sizeHover={};this.progressButtonValue=0;this.iCategory=inditex.xFindCategory(inditex.iCategoryId).pop()||{};this.iQuantity=1;if(this.iProduct&&this.productHelpers.hideOneSize(this.iProduct)){for(var set=!1,i=0;i<this.iProduct.colors.length&&!set;i++)"in_stock"!==this.iProduct.colors[i].sizes[0].stock&&(set=!0);this.showSizeGuide=this.showSizes=set}this._bindEvents();
this._requestConfig();this._checkBestSeller()};productDetailElementCtrl.prototype._checkBestSeller=function(){this.bestSellerCategory=null;if(this.iProduct.relatedCategories&&this.iProduct.relatedCategories.length)for(var i=0,len=this.iProduct.relatedCategories.length;i<len;i++){var category=this.iProduct.relatedCategories[i];if(category._config&&category._config.isBestSeller){this.bestSellerCategory=category;break}}};productDetailElementCtrl.prototype.goToBestSeller=function(){this.navigationFactory.setCategory(this.bestSellerCategory)};
productDetailElementCtrl.prototype._requestConfig=function(){var self=this;this.config.get(["SEASONS_STOCK_QUERY_NOT_ALLOWED","ALTERNATIVE_CURRENCY","SEASONS_BACK_COMING_NOT_ALLOWED","CARES_STATIC_PATH"]).then(function(values){var refSplitted=self.iProduct.reference.split("-"),productSeason=refSplitted&&1<refSplitted.length?refSplitted[1]:null,seasonStockQueryNotAllowed=values[0]?values[0].replace(/\s/g,"").split(","):[];self.showSearchStore&&(self.showSearchStore=inditex.iStoreJSON.details.hasPhisicalStores&&
inditex.iStoreJSON.details.availabilityStockStore&&productSeason&&-1==seasonStockQueryNotAllowed.indexOf(productSeason));try{inditex.alternativeCurrency=JSON.parse(values[1])}catch(e){}var disabledBackSoonComingSoonSeasons=values[2]?values[2].replace(/\s/g,"").split(","):[];self.isBackSoonCommingSoonDisabled=!inditex.iStoreJSON.details.stockNotification||-1!=disabledBackSoonComingSoonSeasons.indexOf(productSeason);self._prepareProduct();self.RTBHouseService.productEvent(self.iProduct,self.iColor?
self.iColor.id:null);self.caresStaticPath=values[3]})};productDetailElementCtrl.prototype.$onDestroy=function(){$(document).off(bershka.events.AddToCart+".product-detail-element")};productDetailElementCtrl.prototype._bindEvents=function(){var self=this;$(document).off(bershka.events.AddToCart+".product-detail-element");$(document).on(bershka.events.AddToCart+".product-detail-element",function(){self.$timeout(function(){self.progressButtonValue=0})})};
productDetailElementCtrl.prototype.openBox=function(box,$event){var $box=this.$element.find(box);box.length&&inditex.popupHtml($box.html(),600,"auto",null,null,"modal",!1)};
productDetailElementCtrl.prototype.selectColor=function(color){if(this.iColor!=color){var activeMocaco=this.productHelpers.getActiveMocaco(this.iProduct.id,color.id,this.gridOrchestrator.getProducts());activeMocaco&&(this.iProduct=activeMocaco,this.hashTags=inditex.getHashTags(activeMocaco),color=activeMocaco.colors[0]);this._selectColor(color);this.trackingService.trackEvent({name:"seleccionar-color",data:{color:color,product:this.iProduct,source:"detailed"}})}};
productDetailElementCtrl.prototype.showMessageFor=function(size){if(this.isOpenForSale){var tooltipType=null,tooltipType="back_soon"!=size.stock||this.isBackSoonCommingSoonDisabled?"coming_soon"!=size.stock||this.isBackSoonCommingSoonDisabled?size.isLowStock?"lowStock":null:"comingSoon":"backSoon";this.sizeHover[size.name]=tooltipType}};productDetailElementCtrl.prototype.hideMessageFor=function(size){this.sizeHover[size.name]=null};productDetailElementCtrl.prototype.showTooltip=function(){return this.tooltip};
productDetailElementCtrl.prototype.selectSize=function(size){this.isOpenForSale&&(this.isDisabledSize(size)||this.isBackSoonSize(size)?!this.isBackSoonCommingSoonDisabled&&this.isBackSoonSize(size)&&this.openEmailMeModal(size):(this.iSize=size,this._refreshSizePrices(),this.bShowSelectSizeMessageWishlist=this.bShowSelectSizeMessage=!1,this._showLowStockMessageOnMobile(size),this.trackingService.trackEvent({name:"seleccionar-talla",data:{product:this.iProduct,productSize:this.iSize,source:"detailed"}})))};
productDetailElementCtrl.prototype.getSizeClass=function(size){var clazz;this.isOpenForSale?"in_stock"==size.stock?clazz=this.iSize&&size.sku==this.iSize.sku?"active":"selectable":(clazz="unavailable",!this.isBackSoonCommingSoonDisabled&&this.isBackSoonSize(size)&&(clazz+=" notifyMe")):clazz="disabled";if(this.bShowSelectSizeMessage||this.bShowSelectSizeMessageWishlist)clazz+=" noSelected";return clazz};productDetailElementCtrl.prototype.openSizeGuideModal=function(){inditex.xOpenSizeGuideModal(this.iProduct.originalProduct.section)};
productDetailElementCtrl.prototype.addToCart=function(){if(!(this.bAllSizesDisabled||this.bAllSizesBackSoon||this.iProduct.priceZero))if(this.iSize){var self=this;this.progressButtonValue=.5;var category=this.categoryFactory.oCategory||{},image=this.iColor?this.iColor.originalColor.image:null,partNumber=this.iSize.partnumber.substr(0,11),item={sku:this.iSize.sku,parentId:parseInt(this.iProduct.bundleId||this.iProduct.id,10),categoryId:inditex.iCategoryId,name:this.iProduct.name,color:this.iColor.name,
