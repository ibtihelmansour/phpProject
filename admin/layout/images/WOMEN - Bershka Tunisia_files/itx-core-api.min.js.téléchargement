ItxTrackingService.prototype.sendTrackedPageviewHelper=function(pageName,customData,saveLast,bForced){var forced=bForced||!1;if(forced||!this.isTheSamePageView(pageName,customData))this.cleanPageViewDatalayer(),pageName||(pageName=this.getLastPageViewName()),customData||(customData=this.getLastPageViewCustomData()),this.addCustomDataToTrackingService(customData),this.pageViewLauncher(pageName),saveLast&&this.saveCurrentPageView(pageName,customData)};
ItxTrackingService.prototype.isTheSamePageView=function(pageName,customData){return-1==this.whiteListPage.indexOf(pageName)?pageName===this.lastPageViewName:"page-detalle-producto"==pageName&&this.lastPageViewCustomData&&this.lastPageViewCustomData.product&&this.product?this.lastPageViewCustomData.product.id==this.product.id:customData.idCategory&&customData.idCategory==this.getLastPageViewCategoryIdData()};
ItxTrackingService.prototype._isABundleChildren=function(){return customData.product&&customData.product.bundleId==customData.product.parentId?!0:!1};ItxTrackingService.prototype.getLastPageViewName=function(){return this.lastPageViewName||"default"};ItxTrackingService.prototype.getLastPageViewCustomData=function(){return this.lastPageViewCustomData||{}};
