angular.module("itxApp").component("bskVideo",{bindings:{urls:"\x3cvideoUrls",poster:"\x3cvideoPoster",loops:"@?",finished:"\x26?",width:"@?",height:"@?"},template:"",controller:["$element","$attrs","$scope","bskScrollFactory","bskVideoEnumeratorFactory","bskVideoHelperFactory","bskVideoConstants",bskVideoCtrl]});
function bskVideoCtrl($element,$attrs,$scope,scrollFactory,videoEnumerator,videoHelpers,videoConstants){this.$element=$element;this.$scope=$scope;this.scrollFactory=scrollFactory;this.videoEnumerator=videoEnumerator;this.videoHelpers=videoHelpers;this.videoConstants=videoConstants;this.controls=$attrs.hasOwnProperty("controls");this.autoplay=$attrs.hasOwnProperty("autoplay");this.loop=$attrs.hasOwnProperty("loop");this.muted=$attrs.hasOwnProperty("muted");this.preload=$attrs.preload||"none";this.checkViewport=
$attrs.hasOwnProperty("onlyInViewport");this.inline=$attrs.hasOwnProperty("inline");this.id=$attrs.id;this.init.apply(this,arguments)}bskVideoCtrl.prototype.init=function(){};bskVideoCtrl.prototype.scrollPause=function(){this.state==this.videoEnumerator.STATE_PLAYING&&(this.state=this.videoEnumerator.STATE_SCROLL_PAUSED,this.videoElement[0].pause())};bskVideoCtrl.prototype.scrollPlay=function(){this.state==this.videoEnumerator.STATE_SCROLL_PAUSED&&(this.state=this.videoEnumerator.STATE_PLAYING,this.videoElement[0].play())};
bskVideoCtrl.prototype.$onInit=function(){if(this.checkViewport){var self=this;this.scrollWatcher=this.scrollFactory.addElement(this.$element,function(inViewport){inViewport?self.scrollPlay():self.scrollPause()},!0,0)}this.maxLoops=0;this.loops&&(this.maxLoops=parseInt(this.loops,10),isNaN(this.maxLoops)&&(this.maxLoops=1));this.maxLoops&&(this.loop=!1);this.maxLoops||this.loop||(this.maxLoops=1);this.videoEnumerator.add(this.id,this);this.bindToElement()};
bskVideoCtrl.prototype.bindToElement=function(){var $el=this.$element.get(0);$el.play=this.play.bind(this);$el.pause=this.pause.bind(this);var self=this;$el.toggle=function(){self.state==self.videoEnumerator.STATE_PAUSED||self.state==self.videoEnumerator.STATE_STOPPED||self.state==self.videoEnumerator.STATE_SCROLL_PAUSED?self.play():self.pause()}};bskVideoCtrl.prototype.$onDestroy=function(){this.scrollWatcher&&this.scrollFactory.removeElement(this.scrollWatcher);this.videoEnumerator.remove(this)};
bskVideoCtrl.prototype.addSources=function($vidElement,urls,poster){if(urls){$.isArray(urls)||(urls=[urls]);this.lastUrlsValues=urls;for(var i=0,len=urls.length;i<len;i++){var url=urls[i],mime=this.videoHelpers.getMime(url);mime&&this.videoConstants.enableMimeTypes?$vidElement.append("\x3csource src\x3d'"+url+"' type\x3d'"+mime+"'/\x3e"):$vidElement.append("\x3csource src\x3d'"+url+"' /\x3e")}}poster&&(this.lastPosterValue=poster,$vidElement.attr("poster",poster),$vidElement.append("\x3cimg src\x3d'"+
poster+"' /\x3e"))};
bskVideoCtrl.prototype.createVideoElement=function(urls,poster){this.$element.empty();if(urls||poster){var $vidElement=$("\x3cvideo /\x3e").prop({controls:this.controls,loop:this.loop,autoplay:this.autoplay,muted:this.muted}).attr({preload:this.preload,width:this.width,height:this.height,controlsList:"nodownload"});this.inline&&$vidElement.attr("playsinline","");this.addSources($vidElement,urls,poster);this.videoElement=$vidElement;this.bindEvents();this.$element.append($vidElement);this.state=this.autoplay?
this.videoEnumerator.STATE_PLAYING:this.videoEnumerator.STATE_SCROLL_PAUSED;$vidElement[0].load()}else this.videoElement=null};
bskVideoCtrl.prototype.bindEvents=function(){this.currentLoop=0;var self=this;if(!this.loop)this.videoElement.on("ended",function(){self.currentLoop++;self.currentLoop<self.maxLoops?(self.state=self.videoEnumerator.STATE_STOPPED,self.play()):(self.stop(),self.$scope.$applyAsync(function(){self.finished()}))});this.duration=-1;this.videoElement.on("durationchange",function(){self.duration=self.videoElement[0].duration});this.videoElement.on("pause",function(){self.state==self.videoEnumerator.STATE_PLAYING&&
(self.state=self.videoEnumerator.STATE_PAUSED)});this.videoElement.on("play",function(){self.state=self.videoEnumerator.STATE_PLAYING});this.videoElement.on("volumechange",function(){self.muted=self.videoElement[0].muted});this.videoElement.on("loadstart",function(){self.loaded="loading"});this.videoElement.on("canplay",function(){self.loaded="canplay"});this.videoElement.on("canplaythrough",function(){self.loaded="loaded"})};
bskVideoCtrl.prototype.$onChanges=function(changes){var urls=changes.urls?changes.urls.currentValue:this.lastUrlsValues,poster=changes.poster?changes.poster.currentValue:this.lastPosterValue;this.createVideoElement(urls,poster)};bskVideoCtrl.prototype.play=function(){this.videoElement&&(this.state==this.videoEnumerator.STATE_STOPPED&&this.seekTo(0),this.state=this.videoEnumerator.STATE_PLAYING,this.videoElement[0].play())};
bskVideoCtrl.prototype.pause=function(){this.videoElement&&(this.state=this.videoEnumerator.STATE_PAUSED,this.videoElement[0].pause())};bskVideoCtrl.prototype.stop=function(){this.videoElement&&(this.state=this.videoEnumerator.STATE_STOPPED,this.videoElement[0].pause())};bskVideoCtrl.prototype.setVideoProp=function(prop,value){this.videoElement&&this.videoElement.prop(prop,value)};bskVideoCtrl.prototype.toggleVideoProp=function(prop){this.videoElement&&this.setVideoProp(prop,!this.videoElement.prop(prop))};
bskVideoCtrl.prototype.muted=function(state){this.setVideoProp("muted",state);this.muted=state};bskVideoCtrl.prototype.seekTo=function(pos){-1<this.duration&&(0>pos?pos=0:100<pos&&(pos=100),this.setVideoProp("currentTime",this.duration*pos/100))};
