(action.params.guestToken=inditex._getURLParameter("guestToken"),action.params.orderId=inditex._getURLParameter("orderId"));var self=this;inditex.execute({actions:[action],onFinish:function(status,response){if(1==status)angular.element("#logonForm button").addClass("success"),$("html").css("overflow-y","auto"),self.iScope.iDisableButtons=!0,self.iScope.isModal&&($("#modal").dialog("close"),self.iScope.$applyAsync()),self.trackingService.trackEvent({name:"user-login-ok"}),$(document).trigger(bershka.events.UserLogin,
{status:status,response:response});else{var errorMsg=bershka.getErrorDescription(response)||inditex.text("logon.error");!self.phoneHelpers.validate||response.key!==self.phoneConstants.ERROR.NOT_VALIDATED&&response.key!=self.phoneConstants.ERROR.INVALID_CODE?(inditex.xOpenErrorPopUp(278,"auto",null,errorMsg),self.trackingService.trackEvent({name:"error-servidor-login",data:{response:response}}),$(document).trigger(bershka.events.UserLogin,{status:status,response:response})):response.key==self.phoneConstants.ERROR.INVALID_CODE?
inditex.xOpenErrorPopUp(278,"auto",null,errorMsg,null,function(){self.doPhoneValidation(phone)}):self.doPhoneValidation(phone)}}})};
