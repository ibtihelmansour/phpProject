productDetailElementCtrl.prototype.addToWishList=function(){if(!(inditex.iUserJSON.wishCart&&this.iProduct&&inditex.xIsProductInWishList(this.iProduct.id)||this.bAllSizesDisabled))if(this.iSize){var category=this.categoryFactory.oCategory,item={sku:this.iSize.sku,parentId:this.iProduct.id,categoryId:inditex.iCategoryId,name:this.iProduct.name,color:this.iColor.name,quantity:1,unitPrice:this.iSize.price,image:this.iColor.image,reference:this.iProduct.reference,productType:this.iProduct.productType,
