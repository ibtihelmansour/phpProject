inditex.isOpenForSale=function(){return inditex.iStoreJSON.isOpenForSale&&2!=inditex.iStoreJSON.type};
inditex.xFindCategoryByProperty=function(property,find,categories){var self=this,result=[];if("undefined"===typeof find)return result;"undefined"===typeof categories&&(categories=inditex.iCategoryTreeJSON?inditex.iCategoryTreeJSON.categories||[]:[]);for(var i=0,len=categories.length;i<len;i++)if(categories[i][property]==find){result=[categories[i]];break}else if(categories[i].subcategories.length){var subtreeResult=self.xFindCategoryByProperty(property,find,categories[i].subcategories);if(subtreeResult.length){result=
[categories[i]].concat(subtreeResult);break}}return result};inditex.xFindCategoryKey=function(selectedKey,categories){return inditex.xFindCategoryByProperty("key",selectedKey,categories)};inditex.xFindCategory=function(selectedId,categories){"undefined"===typeof selectedId&&(selectedId=inditex.iCategoryId);return inditex.xFindCategoryByProperty("id",selectedId,categories)};
inditex.xSetProductState=function(product){if("undefined"!==typeof product.detail){var newProductState="temporada",priceRanges=inditex.rangePriceProduct(product);"undefined"===typeof priceRanges.productId||priceRanges.productId!==product.id||null===priceRanges.minOldPrice&&null===priceRanges.maxOldPrice||(newProductState="saldo");product.detail.productState=newProductState}};
inditex.xDrawBreadcrumb=function(aCategoryId){var self=this;aCategoryId&&null!=aCategoryId&&void 0!=aCategoryId||(aCategoryId=inditex.iCategoryId);var hierarchy=self.xFindCategory(aCategoryId,self.iCategoryTreeJSON.categories);if(hierarchy&&null!=hierarchy){var root=$(".breadcrumb");root.empty();$.each(hierarchy,function(index,category){self.xDrawBreadcrumbElem(root,category);if(category.id==aCategoryId)return!1})}};
inditex.xDrawBreadcrumbElem=function(root,category){var urlCategory=category,findFirstLeafCategory=function(){if(urlCategory.subcategories&&0<urlCategory.subcategories.length){a:{for(var subcategories=urlCategory.subcategories,subcategoriesLength=subcategories.length,i=0;i<subcategoriesLength;i++){var notAcceptedTypes=["1","4","29","30","33"];if(-1===notAcceptedTypes.indexOf(subcategories[i].type)){urlCategory=subcategories[i];break a}}urlCategory=subcategories[0]}findFirstLeafCategory()}};findFirstLeafCategory();
if(category&&null!=category){var options={action:"ItxCategoryPage",params:{storeId:inditex.iStoreId,langId:inditex.iLangId,catalogId:inditex.iCatalogId,categoryId:urlCategory.id},isAjax:!1},url=inditex.encodeUrl(options),element=$("\x3ca\x3e\x3c/a\x3e").attr({href:url}).append(category.name+" "),span=$("\x3cspan\x3e\x3c/span\x3e").append(element);span.appendTo(root)}};
inditex.xFormatReferenceItem=function(reference){var separator="",referenceDisplay="";reference&&(referenceDisplay+=reference.substring(0,8)+separator);return referenceDisplay};inditex.getDiscountToApply=function(range){var deltaMin=inditex.xGetDeltaPrice(range.minOldPrice,range.minPrice),deltaMax=inditex.xGetDeltaPrice(range.maxOldPrice,range.maxPrice);return Math.floor(100*Math.max(deltaMin,deltaMax))};
inditex.rangePriceBundleProduct=function(bundleProduct,visibilityMap){var minPrice=null,maxPrice=null,minOldPrice=null,maxOldPrice=null,rangeProductBundle={},products=[],hasPriceCut=!1;if(bundleProduct&&bundleProduct.bundleProductSummaries&&0<bundleProduct.bundleProductSummaries.length){for(var i=0,len=bundleProduct.bundleProductSummaries.length;i<len;i++){var rangeProduct=this.rangePriceProduct(bundleProduct.bundleProductSummaries[i],visibilityMap);rangeProduct&&(products.push(rangeProduct),minPrice+=
rangeProduct.minPrice,maxPrice+=rangeProduct.maxPrice,hasPriceCut=rangeProduct.minOldPrice||rangeProduct.maxOldPrice,minOldPrice+=rangeProduct.minOldPrice||rangeProduct.minPrice,maxOldPrice+=rangeProduct.maxOldPrice||rangeProduct.maxPrice)}rangeProductBundle.productBundleId=bundleProduct.id;rangeProductBundle.minPrice=minPrice;rangeProductBundle.maxPrice=maxPrice;hasPriceCut&&(rangeProductBundle.minOldPrice=minOldPrice,rangeProductBundle.maxOldPrice=maxOldPrice);rangeProductBundle.type=bundleProduct.type;
rangeProductBundle.discount=inditex.getDiscountToApply(rangeProductBundle);rangeProductBundle.products=products}return rangeProductBundle};
inditex.simpleRangePriceBundleProduct=function(bundleProduct,visibilityMap){var minPrice=null,maxPrice=null,minOldPrice=null,maxOldPrice=null,rangeProductBundle={},products=[],hasPriceCut=!1;if(bundleProduct&&bundleProduct.bundleProductSummaries&&0<bundleProduct.bundleProductSummaries.length){for(var i=0,len=bundleProduct.bundleProductSummaries.length;i<len;i++){var rangeProduct=this.rangePriceProduct(bundleProduct.bundleProductSummaries[i],visibilityMap);rangeProduct&&(products.push(rangeProduct),
minPrice=null===minPrice?rangeProduct.minPrice:Math.min(rangeProduct.minPrice,minPrice),maxPrice=null===maxPrice?rangeProduct.maxPrice:Math.max(rangeProduct.maxPrice,maxPrice),hasPriceCut=rangeProduct.minOldPrice||rangeProduct.maxOldPrice,minOldPrice=null===minOldPrice?rangeProduct.minOldPrice||rangeProduct.minPrice:Math.min(rangeProduct.minOldPrice||rangeProduct.minPrice,minOldPrice||minPrice),maxOldPrice=null===maxOldPrice?rangeProduct.maxOldPrice||rangeProduct.maxPrice:Math.max(rangeProduct.maxOldPrice||
rangeProduct.maxPrice,maxOldPrice||maxPrice))}rangeProductBundle.productBundleId=bundleProduct.id;rangeProductBundle.minPrice=minPrice;rangeProductBundle.maxPrice=maxPrice;hasPriceCut&&(rangeProductBundle.minOldPrice=minOldPrice,rangeProductBundle.maxOldPrice=maxOldPrice);rangeProductBundle.discount=inditex.getDiscountToApply(rangeProductBundle);rangeProductBundle.type=bundleProduct.type;rangeProductBundle.products=products}return rangeProductBundle};
inditex.rangePriceProduct=function(product,visibility){var minPrice=null,maxPrice=null,minOldPrice=null,maxOldPrice=null,range={},hasPrice=!1;product&&("ProductBean"==product.type||"ProductItem"==product.type)&&product.detail&&product.detail.colors&&0<product.detail.colors.length&&($.each(product.detail.colors,function(colorIndex,color){if(color&&color.sizes&&0<color.sizes.length){var hasOldPrice=!1;$.each(color.sizes,function(sizesIndex,size){if(null!=size.oldPrice||size.vipPrice&&inditex.iUserJSON.showPrivateSale)return hasOldPrice=
!0,!1});$.each(color.sizes,function(sizesIndex,size){""!=inditex.getCurrentPrice(size)&&(hasPrice=!0,minPrice=null==minPrice?inditex.getCurrentPrice(size):minPrice,maxPrice=null==maxPrice?inditex.getCurrentPrice(size):maxPrice,minPrice=Math.min(minPrice,inditex.getCurrentPrice(size)),maxPrice=Math.max(maxPrice,inditex.getCurrentPrice(size)));if(hasOldPrice){var priceValue=inditex.getCurrentOldPrice(size);minOldPrice=null==minOldPrice?priceValue:minOldPrice;maxOldPrice=null==maxOldPrice?priceValue:
maxOldPrice;minOldPrice=Math.min(minOldPrice,priceValue);maxOldPrice=Math.max(maxOldPrice,priceValue)}})}}),hasPrice&&(range.productId=product.id,range.minPrice=minPrice,range.maxPrice=maxPrice,range.minOldPrice=minOldPrice,range.maxOldPrice=maxOldPrice,range.discount=inditex.getDiscountToApply(range),range.type=product.type));hasPrice||(range.productId=product.id,range.minPrice=0,range.maxPrice=0,range.minOldPrice=0,range.maxOldPrice=0,range.discount=0,range.type=product.type);return range};
inditex.getCurrentPrice=function(size){return inditex.iUserJSON.showPrivateSale?size.vipPrice||size.price:size.price};inditex.getCurrentOldPrice=function(size){return inditex.iUserJSON.showPrivateSale?size.vipPrice?null==size.oldPrice?size.price:size.oldPrice:size.oldPrice:size.oldPrice};
inditex.ixGetSimplePriceStr=function(price,useSmallDecimalFormat,method,showFree){method=method||inditex.getPriceHtml;return 0===price&&showFree?"\x3cspan class\x3d'price-free'\x3e"+inditex.text("orderShipping.free")+"\x3c/span\x3e":method(inditex.formatPrice(price),useSmallDecimalFormat)};
inditex.ixGetPriceStr=function(rangeProductPrice,useSmallDecimalFormat,method,showFree){var priceStr="";return priceStr=void 0==rangeProductPrice.minPrice||null==rangeProductPrice.minPrice?void 0==rangeProductPrice.maxPrice||null==rangeProductPrice.maxPrice?inditex.text("productPage.noPrice"):inditex.text("productPage.noPrice")+" - "+inditex.ixGetSimplePriceStr(rangeProductPrice.minPrice,useSmallDecimalFormat,method,showFree):rangeProductPrice.minPrice==rangeProductPrice.maxPrice?inditex.ixGetSimplePriceStr(rangeProductPrice.minPrice,
useSmallDecimalFormat,method,showFree):void 0==rangeProductPrice.maxPrice||null==rangeProductPrice.maxPrice||0==parseInt(rangeProductPrice.maxPrice)?inditex.ixGetSimplePriceStr(rangeProductPrice.minPrice,useSmallDecimalFormat,method,showFree)+" - "+inditex.text("productPage.noPrice"):inditex.ixGetSimplePriceStr(rangeProductPrice.minPrice,useSmallDecimalFormat,method,showFree)+" - "+inditex.ixGetSimplePriceStr(rangeProductPrice.maxPrice,useSmallDecimalFormat,method,showFree)};
inditex.getPriceHtml=function(price,useSmallDecimalFormat,showTotalLabel){var decimalChar=inditex.iStoreJSON.details.locale.currencydecimalCharHtml||".",arr=price.split(decimalChar),totalLabel=showTotalLabel?'\x3cspan class\x3d"mc-total-text"\x3eTotal\x3c/span\x3e':"";return 1<arr.length?-1!=arr[1].indexOf(inditex.iStoreJSON.details.locale.currencySymbolHtml)?totalLabel+'\x3cspan class\x3d"integer"\x3e'+arr[0]+'\x3c/span\x3e\x3cspan class\x3d"decimal"\x3e'+decimalChar+arr[1].replace(inditex.iStoreJSON.details.locale.currencySymbolHtml+
"\x26nbsp;","").replace(inditex.iStoreJSON.details.locale.currencySymbolHtml,"")+'\x3c/span\x3e\x3cspan class\x3d"integer"\x3e'+inditex.iStoreJSON.details.locale.currencySymbolHtml+"\x3c/span\x3e":totalLabel+'\x3cspan class\x3d"integer"\x3e'+(0==arr[0].indexOf("\x26nbsp;")?arr[0].replace("\x26nbsp;",""):arr[0])+'\x3c/span\x3e\x3cspan class\x3d"decimal"\x3e'+decimalChar+arr[1]+"\x3c/span\x3e":totalLabel+'\x3cspan class\x3d"integer"\x3e'+(0==arr[0].indexOf("\x26nbsp;")?arr[0].replace("\x26nbsp;",""):
arr[0])+'\x3c/span\x3e\x3cspan class\x3d"decimal"\x3e\x3c/span\x3e'};
inditex.getBasicPriceHtml=function(price,useSmallDecimalFormat){if(useSmallDecimalFormat){var decimalChar=inditex.iStoreJSON.details.locale.currencydecimalCharHtml||".",arr=price.split(decimalChar);if(1<arr.length)return-1!=arr[1].indexOf(inditex.iStoreJSON.details.locale.currencySymbolHtml)?arr[0]+'\x3cspan class\x3d"decimal"\x3e'+decimalChar+arr[1].replace(inditex.iStoreJSON.details.locale.currencySymbolHtml+"\x26nbsp;","").replace(inditex.iStoreJSON.details.locale.currencySymbolHtml,"")+"\x3c/span\x3e"+
inditex.iStoreJSON.details.locale.currencySymbolHtml:(0==arr[0].indexOf("\x26nbsp;")?arr[0].replace("\x26nbsp;",""):arr[0])+'\x3cspan class\x3d"decimal"\x3e'+decimalChar+arr[1]+"\x3c/span\x3e"}return price};
inditex.getPricesHtml=function(rangeProductPrice,useSmallDecimalFormat,deltaPosition,showFree){var productPriceClass="",oldPrice="",percentDelta="";rangeProductPrice.minOldPrice&&(oldPrice+='\x3cspan class\x3d"productPrice oldPrice"\x3e'+inditex.getPriceHtml(inditex.formatPrice(rangeProductPrice.minOldPrice),useSmallDecimalFormat),rangeProductPrice.maxOldPrice&&rangeProductPrice.minOldPrice!=rangeProductPrice.maxOldPrice&&(oldPrice+=" - "+inditex.getPriceHtml(inditex.formatPrice(rangeProductPrice.maxOldPrice),
useSmallDecimalFormat)),oldPrice+="\x3c/span\x3e",percentDelta+=inditex.xGetDeltaPriceHtml(rangeProductPrice.minOldPrice,rangeProductPrice.maxPrice),productPriceClass=" newPrice");var productPrice='\x3cspan class\x3d"productPrice'+productPriceClass+'"\x3e'+inditex.ixGetPriceStr(rangeProductPrice,useSmallDecimalFormat,inditex.getPriceHtml,showFree)+"\x3c/span\x3e";return"front"==deltaPosition?percentDelta+oldPrice+productPrice:oldPrice+productPrice+percentDelta};
inditex.getBasicPricesHtml=function(rangeProductPrice,useSmallDecimalFormat,deltaPosition,showFree){var productPriceClass="",oldPrice="",percentDelta="",productPrice="";rangeProductPrice.minOldPrice&&(oldPrice+='\x3cspan class\x3d"productPrice oldPrice"\x3e'+inditex.getBasicPriceHtml(inditex.formatPrice(rangeProductPrice.minOldPrice),useSmallDecimalFormat),rangeProductPrice.maxOldPrice&&rangeProductPrice.minOldPrice!=rangeProductPrice.maxOldPrice&&(oldPrice+=" - "+inditex.getBasicPriceHtml(inditex.formatPrice(rangeProductPrice.maxOldPrice),
useSmallDecimalFormat)),oldPrice+="\x3c/span\x3e",percentDelta=inditex.xGetBasicDeltaPriceHtml(rangeProductPrice.minOldPrice,rangeProductPrice.maxPrice),productPriceClass=" newPrice");rangeProductPrice.maxPrice&&(productPrice='\x3cspan class\x3d"productPrice'+productPriceClass+'"\x3e'+inditex.ixGetPriceStr(rangeProductPrice,useSmallDecimalFormat,inditex.getBasicPriceHtml,showFree)+"\x3c/span\x3e");return"none"==deltaPosition?oldPrice+productPrice:"front"==deltaPosition?percentDelta+oldPrice+productPrice:
oldPrice+productPrice+percentDelta};inditex.xGetDeltaPrice=function(oldPrice,newPrice){var deltaPrice=0,deltaPriceAbs=0,deltaPrice=(newPrice-oldPrice)/oldPrice*100,deltaPriceAbs=Math.abs(deltaPrice);minimum=parseInt(bershka.getStoreConfiguration("minDiscountToShow"),10);isNaN(minimum)&&(minimum=0);deltaPrice=0>deltaPrice&&deltaPriceAbs>=minimum?Math.round(deltaPriceAbs):void 0;return isNaN(deltaPrice)?0:deltaPrice};
inditex.xGetDeltaPriceHtml=function(oldPrice,newPrice){var deltaPriceHtml="",deltaPrice=inditex.xGetDeltaPrice(oldPrice,newPrice);deltaPrice&&(deltaPriceHtml="\x3cspan class\x3d'delta'\x3e\x3cspan class\x3d'integer'\x3e"+deltaPrice+"\x3c/span\x3e\x3cspan class\x3d'delta-symbol'\x3e%\x3c/span\x3e\x3c/span\x3e");return deltaPriceHtml};
inditex.xGetBasicDeltaPriceHtml=function(oldPrice,newPrice){var deltaPriceHtml="",deltaPrice=inditex.xGetDeltaPrice(oldPrice,newPrice);deltaPrice&&(deltaPriceHtml="\x3cspan class\x3d'delta'\x3e"+deltaPrice+"%\x3c/span\x3e");return deltaPriceHtml};inditex.xGetThresholdMap=function(stock){var thresholdMap=[];if(stock)for(var i=stock.length-1;0<=i;i--)for(var product=stock[i],j=product.stocks.length-1;0<=j;j--){var size=product.stocks[j];thresholdMap[size.id]=size.typeThreshold}return thresholdMap};
inditex.ixGetUniqueSizes=function(sizes,visibilityMap,stockMap){if(!sizes)return[];for(var sizesLength=sizes.length,uniqueSizes=[],priority={hidden:0,sold_out:1,coming_soon:2,back_soon:3,show:4},secondPriority={UNDEFINED_UMBRAL_STOCK:0,BSK_UMBRAL_BAJO:1,BSK_UMBRAL_ALTO:2},i=0;i<sizesLength;i++){for(var exists=!1,size$$0=sizes[i],uniqueSizesLength=uniqueSizes.length,j=0;j<uniqueSizesLength;j++){var uniqueSize=uniqueSizes[j],uname=$.trim(uniqueSize.name),sname=$.trim(size$$0.name);if(uname.toUpperCase()==
sname.toUpperCase()){var exists=!0,uniqueSizePriority=priority[visibilityMap[uniqueSize.sku]],sizePriority=priority[visibilityMap[size$$0.sku]],uniqueSizeSecondPriority=secondPriority[stockMap[uniqueSize.sku]]||0,sizeSecondPriority=secondPriority[stockMap[size$$0.sku]];if(sizePriority>uniqueSizePriority||sizePriority==uniqueSizePriority&&sizeSecondPriority>uniqueSizeSecondPriority)uniqueSizes[j]=sizes[i];break}}exists||uniqueSizes.push(size$$0)}var uniqueSizesName=$.map(uniqueSizes,function(size){return size.name.replace(/\s*\([^)]*\)/g,
"")}),uniqueSizesName=inditex.sortSizes(uniqueSizesName);return uniqueSizes=uniqueSizes.sort(function(a,b){var aName=a.name.split("-")[0],bName=b.name.split("-")[0];return uniqueSizesName.indexOf(aName)-uniqueSizesName.indexOf(bName)})};inditex.ixGetAllUniqueSizes=function(colors,visibilityMap){var allSizes=[];if(colors)for(var i=0,len=colors.length;i<len;i++){var sizes=colors[i].sizes;void 0!=sizes&&$.isArray(sizes)&&(allSizes=allSizes.concat(sizes))}return inditex.ixGetUniqueSizes(allSizes,visibilityMap)};
inditex.getColor=function(colors,colorId,visibilityMap){for(var colorsLength=colors.length,defaultColorIfEmpty=void 0,color=void 0,i=0;i<colorsLength;i++){var stepColor=colors[i];if(!defaultColorIfEmpty&&stepColor.image&&inditex.showColor(stepColor,visibilityMap)){if(!colorId){color=stepColor;break}defaultColorIfEmpty=stepColor}if(stepColor.id==colorId){color=stepColor;break}}color||(color=defaultColorIfEmpty||colors[0]);return color};
inditex.getCompositions=function(product){var tempComposition={},zoneComp={},orderedArray=product.detail.composition;orderedArray.sort(function(a,b){return a.part-b.part});for(var i=0;i<orderedArray.length;i++){var comp=orderedArray[i];tempComposition[comp.part]||(tempComposition[comp.part]=[{name:inditex.text("compositionPart."+product.productType+"."+comp.part),compositions:[]}]);tempComposition[comp.part][0].compositions.push(comp.composition[0])}for(i=0;i<product.detail.compositionByZone.length;i++){var part=
product.detail.compositionByZone[i];if(part.zones)for(var j=0;j<part.zones.length;j++){var zone=part.zones[j],iPart=part.part;zoneComp[iPart]||(zoneComp[iPart]=[]);zoneComp[iPart][zone.zone]||(zoneComp[iPart][zone.zone]={name:zone.zoneName,compositions:[]});zoneComp[iPart][zone.zone].compositions.push(zone.composition[0])}}for(var k in tempComposition)zoneComp[k]||(zoneComp[k]=tempComposition[k]);var returnedComp=[];for(k in zoneComp){var part=zoneComp[k],l;for(l in part)zone=part[l],returnedComp.push(zone)}return returnedComp};
