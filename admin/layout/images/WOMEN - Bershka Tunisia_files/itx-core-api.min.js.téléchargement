categoryDetailCtrl.prototype.isFiltered=function(product){return product.isSeparator?this.gridOrchestrator.isFiltered(product)&&this.showSeparators:this.gridOrchestrator.isFiltered(product)};categoryDetailCtrl.prototype.goToCategory=function(category,$event){$event&&$event.preventDefault();this.navigationFactory.setCategory(category)};categoryDetailCtrl.prototype.hasVisibleProducts=function(){return this.aProducts?this.gridOrchestrator.hasVisibleProducts():!0};
categoryDetailCtrl.prototype.hasProducts=function(){return this.aProducts?0<this.aProducts.length:!1};
