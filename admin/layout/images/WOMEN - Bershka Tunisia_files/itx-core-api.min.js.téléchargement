relatedProductsCtrl.prototype.loadRelated=function(){var id;id=this.color&&this.color.style?this.product.bundleId:"mocaco"==this.product.displayType?this.product.id:bershka.getStoreConfiguration("useBundleInfo")?this.product.bundleId:bershka.getParamFromUrlSingleValue("bundleId")||this.product.id;return this.doLoadRelated(id)};relatedProductsCtrl.prototype.getRelatedDetail=function(id){for(var i=0,len=this.allRelated.length;i<len;i++){var related=this.allRelated[i];if(related.id==id)return related}return null};
relatedProductsCtrl.prototype.prepareRelated=function(){this.activeRelated=[];if(this.color&&this.allRelated&&this.allRelated.length)for(var i=0,len=this.colors.length;i<len;i++){var normalizedColor=this.colors[i];if(normalizedColor.id==this.color.id){for(var j=0,len2=normalizedColor.relatedProducts.length;j<len2;j++){var related=normalizedColor.relatedProducts[j],product=this.getRelatedDetail(related.id);product&&!this.productHelpers.isProductOutOfStock(product,related.colorId)&&(product.selectedColorId=
