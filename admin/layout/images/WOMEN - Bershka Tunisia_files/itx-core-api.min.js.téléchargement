inditex.sortSizes=function(aSizeArray){var self=this;return aSizeArray.sort(function(aSize1,aSize2){for(var points="XXS XS S M L XL XXL".split(" "),a_in_points=!1,b_in_points=!1,i=0;i<points.length;i++)if(points[i]==aSize1){a_in_points=!0;break}for(i=0;i<points.length;i++)if(points[i]==aSize2){b_in_points=!0;break}if(a_in_points&&b_in_points){for(var index1=-1,index2=-1,i=0;i<points.length;i++)aSize1.toUpperCase()==points[i]&&(index1=i),aSize2.toUpperCase()==points[i]&&(index2=i);return-1==index1&&
-1==index2?aSize1<aSize2?-1:aSize1==aSize2?0:1:index1-index2}if(a_in_points)return 1;if(b_in_points)return-1;tmp_a=aSize1.replace(/([0-9]+)/g,"$1#").replace(/([a-zA-Z]+)/g,"$1#").replace(/#$/,"");tmp_b=aSize2.replace(/([0-9]+)/g,"$1#").replace(/([a-zA-Z]+)/g,"$1#").replace(/#$/,"");as=tmp_a.split("#");bs=tmp_b.split("#");for(i=0;i<Math.min(as.length,bs.length);i++){var a_has_number=/[0-9]/.test(as[i]),b_has_number=/[0-9]/.test(bs[i]);if(a_has_number&&b_has_number){if(Number(as[i])>Number(bs[i]))return 1;
if(Number(as[i])<Number(bs[i]))return-1}else{if(as[i]>bs[i])return 1;if(as[i]<bs[i])return-1}}return as.length-bs.length})};
