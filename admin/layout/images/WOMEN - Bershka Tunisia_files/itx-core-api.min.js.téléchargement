ItxTrackingService.prototype._addClickedProduct=function(options,directed){if(directed)return this._addDirectClickedProduct(options);for(var indStr="G"==options.product.reference[0]?options.product.reference.substring(1,9)+options.color.id+"-"+options.product.reference.split("-")[1]+"-"+(options.color.style||"00"):options.product.reference.split("-")[0]+options.color.id+"-"+options.product.reference.split("-")[1]+"-"+(options.color.style||"00"),modelName=options.color&&options.color.originalColor?
