angular.module("itxApp").component("shippingPickup",{templateUrl:"tplShippingPickup",controller:["userAddressesFactory","trackingService","storesFactory","googleBaiduHelper","orderFactory","checkoutCacheFactory",shippingPickupCtrl]});
function shippingPickupCtrl(userAddressesFactory,trackingService,storesFactory,mapHelper,order,cache){this.addressFactory=userAddressesFactory;this.trackingService=trackingService;this.storesFactory=storesFactory;this.mapHelper=mapHelper;this.order=order;this.cache=cache;this.init.apply(this,arguments)}
shippingPickupCtrl.prototype.init=function(){this.storesFactory.selected=this.cache.get("store")||this.storesFactory.selected;var self=this;this.showInstantSelector=!1;this.storesFactory.searchFavourites().finally(function(){self.storesFactory.favourites&&0<self.storesFactory.favourites.length&&(self.showInstantSelector=!0,self.storesFactory.selected&&(self.lastFavouriteStore=self.storesFactory.selected.app.isFavourite?self.storesFactory.selected:self.storesFactory.favourites[0]))})};
shippingPickupCtrl.prototype.$onDestroy=function(){"payment"==this.order.step&&this.cache.remove("store").persist()};
shippingPickupCtrl.prototype.toggleFavourite=function(){if(this.storesFactory.selected&&!this.disableToggle){this.disableToggle=!0;var self=this;this.storesFactory.toggleFavourite(this.storesFactory.selected).then(function(){self.storesFactory.selected.app.isFavourite&&self.trackingService.trackEvent({name:"add-favourite-store",data:{sId:self.storesFactory.selected.id}})}).finally(function(){self.disableToggle=!1})}};
shippingPickupCtrl.prototype.showFavourites=function(geolocate){var self=this;inditex.xOpenStoreLocator(900,600,{showSearch:!1,showAddToFavourites:!1,notifyStoreSelected:!0,showSeeOnMap:!1,searchEcommerce:!0,geoLocate:geolocate,compressedInfo:!0,showFavourites:!0},function(){self.lastFavouriteStore=self.storesFactory.selected;self.cache.set("store",self.storesFactory.selected);self.cache.persist();self.trackingService.trackEvent({name:"select-favourite-store",data:{favouriteStore:self.storesFactory.selected}})})};
shippingPickupCtrl.prototype.suggestionSelected=function(suggestion){this.suggestion=suggestion};
