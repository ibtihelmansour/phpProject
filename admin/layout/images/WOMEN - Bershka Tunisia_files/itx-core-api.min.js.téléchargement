ItxOrderRepaymentPageClass.prototype._preserveState=function(){var cache=this._uncacheData("cache-state")||{};cache.version=this.iCacheStateVersion;cache.timestamp=(new Date).getTime();cache.regNewsletter=this.iScope.regNewsletter;var dataCopy=$.extend({},this.iScope.creditCardData);dataCopy.cvv2=void 0;delete dataCopy.cvv2;cache.payment||(cache.payment={});cache.payment.data=dataCopy;cache.payment.method=this.iScope.selectedPaymentMethod?this.iScope.selectedPaymentMethod.code:null;this._cacheData(cache,
"cache-state")};ItxOrderRepaymentPageClass.prototype._uncacheData=function(store,key){var cache=inditex.getSessionStorage(store)||{},userID=(inditex.iUserJSON.identity.userId||"-1002")+"#"+inditex.iCountryCode;return cache[userID]?key?cache[userID][key]:cache[userID]:null};
