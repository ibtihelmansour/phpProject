relatedProductsCtrl.prototype.doLoadRelated=function(id){var self=this;if(id==this.loadedRelatedId)this.prepareRelated();else return this.executeCache.execute([{action:"catalog:relatedProducts",params:{storeId:inditex.iStoreId,catalogId:inditex.iCatalogId,categoryId:inditex.iCategoryId,productId:id}}]).then(function(results){var related=results[0].value;related.relatedProducts&&related.relatedProducts.length?(self.colors=self.product.colors.map(function(color){return{id:color.id,relatedProducts:[]}}),
