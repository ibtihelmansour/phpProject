ItxGuestOrdersPageClass.prototype._fetchGuestOrders=function(){var self=this,pageSize=bershka.config("GestOrdersPerPage");if(this.iScope.orderTokens&&this.iScope.orderTokens.orders&&this.iScope.orderTokens.orders.length)for(var actions=[],i=0;i<pageSize&&0<this.iScope.orderTokens.orders.length;i++){var order$$0=this.iScope.orderTokens.orders.shift();order$$0.guestToken&&this.executeCache.execute({actions:[{action:"order:orderGuest",params:{storeId:inditex.iStoreId,orderId:order$$0.orderId,languageId:inditex.iLangId,
