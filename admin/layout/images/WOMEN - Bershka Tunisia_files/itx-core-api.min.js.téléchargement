ItxTrackingService.prototype.setCommonPageViewData=function(){this.dataLayer.event="pageview";this.dataLayer.context={platform:this.platform,codeVersion:this.codeVersion,storeId:this.storeId,language:this.ISOlanguage,isOpenStore:this.isOpenStore,countryId:"BERSHKA_"+inditex.iStoreJSON.countryCode};this.dataLayer.user={isLogged:this.isLogged};this.dataLayer.user.userId=this._getUserId();this.dataLayer.user.hashEmail=this._getHashEmail();this.dataLayer.context.countryVisit=this._getCountryVisit()};
ItxTrackingService.prototype._getCountryVisit=function(){var visitedCountries=inditex.getSessionStorage("visitedCountries");if(visitedCountries){var visitedCountriesArray=visitedCountries.split("|");if(visitedCountriesArray.length)var currentCountryVisited=-1!=visitedCountries.indexOf(inditex.iStoreJSON.countryCode)}else visitedCountries="";if(!currentCountryVisited)return this._addVisitedCountry(visitedCountries,inditex.iStoreJSON.countryCode),1};
ItxTrackingService.prototype._addVisitedCountry=function(visitedCountries,country){""!=visitedCountries&&(visitedCountries+="|");inditex.setSessionStorage("visitedCountries",visitedCountries+country)};ItxTrackingService.prototype.setPageData=function(pageName){this.pageViewList[pageName]&&this.pageViewList[pageName].call(this,pageName);this.dataLayer.page={pageURL:this._getPageUrl(),section:this.section,pageType:this.pageType,pageTitle:this.pageTitle}};
