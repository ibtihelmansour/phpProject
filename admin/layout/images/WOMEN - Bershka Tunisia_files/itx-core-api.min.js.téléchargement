productDetailElementCtrl.prototype.openEmailMeModal=function(size){this.trackingService.trackEvent({name:"ver-coming-soon",data:{product:this.iProduct.originalProduct,productColor:this.iColor,productSize:size,type:size.stock}});var url=inditex.encodeUrl({action:"ItxStockNotificationPage",params:{storeId:inditex.iStoreId,langId:inditex.iLangId,catalogId:inditex.iCatalogId,categoryId:inditex.iCategoryId},isAjax:!1}),self=this;inditex.popupUrl(url,648,"auto",function(){inditex.initModalController("itxStockNotificationPage",
{product:self.iProduct.originalProduct,size:size,imageUrl:self.productImageUrl,color:self.iColor})},function(){self.trackingService.setCategoryPath(inditex.iCategoryId);self.trackingService.trackPageview({name:"page-detalle-producto",data:{category:inditex.iCategoryId,product:self.iProduct,modelName:self.iColor.modelName,color:self.iColor}})},"modal")};
productDetailElementCtrl.prototype._refreshSizePrices=function(){this.iSize?(this.iRangeProductPrice.maxPrice=inditex.getCurrentPrice(this.iSize),this.iRangeProductPrice.minPrice=inditex.getCurrentPrice(this.iSize),this.iRangeProductPrice.maxOldPrice=inditex.getCurrentOldPrice(this.iSize),this.iRangeProductPrice.minOldPrice=inditex.getCurrentOldPrice(this.iSize)):this.iRangeProductPrice=this.iColor?this.iColor.price:this.iProduct.price;this.priceZero=!this.iRangeProductPrice||0==this.iRangeProductPrice.maxPrice&&
0==this.iRangeProductPrice.minPrice};productDetailElementCtrl.prototype.getActiveColor=function(){for(var ret,paramColorId=this._getColorId(),i=0,len=this.iProduct.colors.length;i<len;i++){var color=this.iProduct.colors[i];if(paramColorId==color.id)return color;ret||"in_stock"!=color.stock||(ret=color)}return ret||this.iProduct.colors[0]};
productDetailElementCtrl.prototype._prepareProduct=function(){this.iRangeProductPrice=this.iProduct.price;var paramColorId=this._getColorId();inditex.xSetProductState(this.iProduct.originalProduct);this._selectColor(this.getActiveColor());this._setDetailImage();this.iIsProductBean="ProductBean"==this.iProduct.type};
productDetailElementCtrl.prototype._selectColor=function(color){this.iColor=color;this.aSizes=color.sizes?color.sizes:[];this.aProductLabels=this.attributesHelper.findAttributes(this.iProduct,"XLABEL");this.iSize=1==this.iColor.sizes.length&&"out_of_stock"!=this.iColor.stock?this.iColor.sizes[0]:void 0;this.bAllSizesDisabled="out_of_stock"==this.iColor.stock;this.bAllSizesBackSoon="back_soon"==this.iColor.stock||"coming_soon"==this.iColor.stock;this._refreshSizePrices();$(document).triggerHandler(bershka.events.productColorSelected,
