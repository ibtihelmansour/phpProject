ItxAjaxProductPageClass.prototype.selectAvailableSize=function(){var color=this.iScope.iColor,sizes=this.iScope.iColor.sizes;this.iScope.iSizes=this.iScope.iColor.sizes;this.iScope.iSize=void 0;if(this.iScope.isUpdate)for(var i=0,len=sizes.length;i<len;i++){var size=sizes[i],sizeCls=inditex.getSizeClass(size,null,size,!0);if(-1==sizeCls.indexOf("unavailable")&&size.name==this.iScope.selectedSizeName){this.iScope.iSize=size;break}}else{for(var size=null,numVisibleSizes=0,i=0,len=sizes.length;i<len;i++){var visibility=
sizes[i].stock;if("hidden"!=visibility||"sold_out"!=visibility||"out_of_stock"!=visibility)size=sizes[i],numVisibleSizes++}1==numVisibleSizes&&(this.iScope.iSize=size)}};
