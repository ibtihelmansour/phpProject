sendedStatusCount<order.suborders.length&&(status="USER2")}return status},setActiveOrder:function(index,order){this.iScope.orderDetail!=index?(this.iScope.orderDetail=index,this.trackingService.trackEvent({name:"orders-guest-order-details",data:{order:order}})):this.iScope.orderDetail=void 0},setReturnOrder:function(order){this.iScope.selectedOrder=order;this.iScope.returnOrderId=order.id;this.iScope.returnGuestToken=order.guestToken;this.iScope.isAutoLoginOrder=order.autoLoginEnabled;this.trackingService.trackEvent({name:"orders-guest-return-order",
