bskMapCtrl.prototype.drawMarkers=function(){var toAdd=[],toRemove=[];this.closeWindows();for(var i=0,len=this.elements.length;i<len;i++){for(var onMap=!1,j=0,len2=this.drawnElements.length;j<len2&&!onMap;j++)onMap=this.drawnElements[j].id==this.elements[i].id;onMap||toAdd.push(this.elements[i])}for(i=this.drawnElements.length;i--;){for(var onList=!1,j=0,len2=this.elements.length;j<len2&&!onList;j++)onList=this.drawnElements[i].id==this.elements[j].id;onList||(this.map.removeMarker(i),this.drawnElements.splice(i,
1))}this.drawnElements=this.drawnElements.concat(toAdd);i=0;for(len=toAdd.length;i<len;i++)this.addMarker(toAdd[i])};
