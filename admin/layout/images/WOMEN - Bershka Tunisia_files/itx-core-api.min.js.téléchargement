olapicCtrl.prototype.openModal=function(item,ev){ev.preventDefault();this.olapicService.selected=item};olapicCtrl.prototype.moveNext=function(ev){ev&&ev.preventDefault();var self=this,width=this._getItemWidth(),carousel=this.carousel||this.element.find(".olapicCar_carousel");carousel.css("left",0);carousel.animate({left:-1*width},300,function(){self._moveFirstItem();carousel.css("left",0)})};
olapicCtrl.prototype.movePrev=function(ev){ev&&ev.preventDefault();var self=this,width=this._getItemWidth(),carousel=this.carousel||this.element.find(".olapicCar_carousel");carousel.css("left",-1*width);self._moveLastItem();carousel.animate({left:0},300,function(){})};olapicCtrl.prototype._getItemWidth=function(){return this.element.find(".olapicCar_carousel li:first-child").outerWidth(!0)};
olapicCtrl.prototype._moveFirstItem=function(){var carousel=this.carousel||this.element.find(".olapicCar_carousel");carousel.append($(".olapicCar_carousel li:first-child")).append(this.element.find(".olapicCar_addPhotoSlide"))};olapicCtrl.prototype._moveLastItem=function(){var carousel=this.carousel||this.element.find(".olapicCar_carousel");carousel.prepend($(".olapicCar_carousel li.olapic_item").eq(-1)).append(this.element.find(".olapicCar_addPhotoSlide"))};
olapicCtrl.prototype.upload=function(ev){ev.preventDefault();var html='\x3ciframe id\x3d"v1iframe" src\x3d"'+this.uploaderSrc+inditex.iLocale+'" style\x3d"zoom:1" width\x3d"100%" height\x3d"500" frameborder\x3d"0"\x3e\x3c/iframe\x3e';inditex.popupHtml(html,700,580,null,null,"modal")};olapicCtrl.prototype._loadBindings=function(){var self=this,_debounce=$.debounce(function(){self._recalculateWidth()},50);$(window).on("resize.olapic",_debounce)};olapicCtrl.prototype.$onDestroy=function(){$(window).off("resize.olapic")};
olapicCtrl.prototype._recalculateWidth=function(){var self=this;if(this.items&&this.items.length){var carouselWidth=this.element.find(".olapicCar_listContainer").width(),itemWidth=this._getItemWidth(),addPhotowidth=carouselWidth-itemWidth*this.items.length-12;this.element.find(".olapicCar_addPhotoSlide").width(addPhotowidth);this.$timeout(function(){self.hideAddPhotoSlide=itemWidth/2>addPhotowidth?!0:!1})}};
