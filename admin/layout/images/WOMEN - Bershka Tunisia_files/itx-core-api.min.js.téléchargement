self.iScope.iSearchPointsCity,callback)}};
ItxAjaxDeliveryPointPageClass.prototype._searchMx=function(){var self=this;inditex.execute({actions:[{action:"bam:dropPointItx",params:{storeId:inditex.iStoreId,languageId:inditex.iLangId,countryCode:inditex.iStoreJSON.countryCode,state:self.iScope.iState,municipality:self.iScope.iMunicipality,colony:self.iScope.iColony,postalCode:self.iScope.iPostalCode}}],onFinish:function(status,points){1==status?0<points.dropPoints.length?(self.iScope.iSearchedPoints=points.dropPoints,$.each(points.dropPoints,
