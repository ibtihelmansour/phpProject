ItxOrderRepaymentPageClass.prototype._getPaymentCard=function(isInstantCheckout){var card=null;if(isInstantCheckout)card={cardHash:this.iScope.instantPaymentCard.hash,paymentMethodKind:"walletcard",paymentMethodType:"walletcard"},this.iScope.instantPaymentCard.instantPaymentMode&&(card.paymentModeId=this.iScope.instantPaymentCard.instantPaymentMode),this._isTrustedPayment(this.iScope.instantPaymentCard)||("affinity"==this.iScope.selectedPaymentMethod.kind.toLowerCase()?card.number=this.iScope.instantPaymentCard.trustedPaymentInfo||
