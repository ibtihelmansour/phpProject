shopCartItemCtrl.prototype._updateRange=function(){var range=this.iRange||{},price,oldPrice;this.iEditMode?(price=this.selectedSize.price*this.selectedQuantity,this.selectedSize.oldPrice&&(oldPrice=this.selectedSize.oldPrice*this.selectedQuantity)):(price=this.iProduct.unitPrice*this.iProduct.quantity,this.iProduct.unitComparePrice&&(oldPrice=this.iProduct.unitComparePrice*this.iProduct.quantity));if(price!=range.maxPrice||oldPrice!=range.maxOldPrice)this.iRange={minPrice:price,minOldPrice:oldPrice,
maxPrice:price,maxOldPrice:oldPrice}};
