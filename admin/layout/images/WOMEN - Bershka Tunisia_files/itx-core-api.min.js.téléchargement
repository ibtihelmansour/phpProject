shopCartItemCtrl.prototype.getProductURL=function(){var item=this.iProduct;if(this.isGiftCard)return inditex.xIsPhysicalCard(item)?inditex.xGetPhysicalCardPage():inditex.xGetGiftCardVirtualPage();if(this.isGiftWrap)return"#";var stylismId;item.hasStyle&&(stylismId=item.style);return this.unifiedProduct?inditex.getUniqueProductUrl(item.productUrl,item.productUrlParam,item.colorId,stylismId,item.categoryId):inditex.getProductUrl(item.parentId,item.colorId,null,item.categoryId+"",item.name,stylismId)};
shopCartItemCtrl.prototype.remove=function(){function commitRemove(){self.iEditInProgress=!0;self.shopCartService.updateItem(product,{quantity:0},function(status){self.iEditInProgress=!1;1==status?(self.trackingService.trackEvent({name:"cesta-eliminar",data:{product:product}}),self.trackingService.trackPageviewForced({name:"page-view-shopcart"})):inditex.xOpenErrorPopUp(278,"auto")})}var self=this,product=this.iProduct;this.isOutOfStock()?commitRemove():(self.trackingService.setPopupUpSilent(!0),
