(function(){var oldSize,oldColor,oldQuantity;shopCartItemCtrl.prototype.$doCheck=function(){if(this.iEditMode){this.selectedSize&&this.selectedSize!=oldSize&&(oldSize=this.selectedSize,0!=this.iEditInProgress||"back_soon"!=oldSize.stock&&"coming_soon"!=oldSize.stock||(this.iEditInProgress=!0,this.openEmailMeModal(oldSize)));if(this.selectedColor&&this.selectedColor!=oldColor){oldColor=this.selectedColor;for(var name=this.selectedSize.name,i=0,len=oldColor.sizes.length;i<len;i++){var size=oldColor.sizes[i];
