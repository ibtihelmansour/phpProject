shopCartItemCtrl.prototype.closeDetail=function(){this.iEditMode=!1;var product=this.iProduct;if(this.selectedSize&&this.selectedColor){var item;product.sku!=this.selectedSize.sku?item={sku:this.selectedSize.sku,size:this.selectedSize.name,unitPrice:this.selectedSize.price,color:this.selectedColor.name,colorId:this.selectedColor.id,image:this.selectedColor.image,parentReference:product.parentReference,quantity:this.selectedQuantity}:product.quantity!=this.selectedQuantity&&(item={quantity:this.selectedQuantity});
