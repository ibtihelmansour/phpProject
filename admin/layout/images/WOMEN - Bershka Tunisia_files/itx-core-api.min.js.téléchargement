ItxOrderDetailPageCtrl.prototype.redirectToLoginPage=function(){inditex.goToPage("ItxGuestOrdersValidationPage",{checkoutButtonAction:"ItxGuestOrdersPage",secondaryAction:"returns"})};
ItxOrderDetailPageCtrl.prototype._loadOrders=function(){var orderId=this.stateFactory.search("orderId");if(void 0!=orderId){var token=this.stateFactory.search("token");this.iScope.orderTokens=this.iScope.orderTokens||{};this.iScope.orderTokens.orders=[];this.iScope.orderTokens.orders.push({orderId:orderId,guestToken:token});this.stateFactory.search("token",null).replace();this.stateFactory.search("orderId",null).replace()}};
ItxOrderDetailPageCtrl.prototype._bindings={getProductImage:function(item){var image=this.imageService.getProductDetailImages(item,{id:item.colorId},this.imageConstants.locations.CHECKOUT,this.imageConstants.sizes.SMALL,item.style)[0];return image?image.src:""},getColorImage:function(item){var stylismId=item.origin&&item.origin.length?item.origin[0].style:item.style,image=this.imageService.getProductDetailImages(item,{id:item.colorId},this.imageConstants.locations.COLORCUT,this.imageConstants.sizes.SMALL,
