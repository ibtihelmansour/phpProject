angular.module("itxApp").component("bskMap",{bindings:{elements:"\x3c?",focusOn:"\x3c?",onAction:"\x26?",actions:"\x3c?",type:"@"},templateUrl:"tplBskMap",controller:["$element","$timeout","trackingService","googleBaiduHelper","$q",bskMapCtrl]});
function bskMapCtrl($element,$timeout,trackingService,mapHelper,$q){this.initiated=null;this.$element=$element;this.$timeout=$timeout;this.$q=$q;this.trackingService=trackingService;this.mapHelper=mapHelper;this.map=null;this.drawnElements=[];this.init.apply(this,arguments)}bskMapCtrl.prototype.hasAction=function(action){return this.actions&&-1!=this.actions.indexOf(action)};
bskMapCtrl.prototype.getStoreMarkerHtml=function(store){if(store&&store.addressLines&&0<store.addressLines.length&&store.city&&store.zipCode){var html="\x3cdiv class\x3d'store-info-container "+(this.hasAction("select")?"live-event-bind-select":"")+"'\x3e\x3cdiv class\x3d'store-info'\x3e",useFavourite=inditex.iStoreJSON.details.showFavouriteStores&&"G"!==inditex.iUserJSON.userType&&this.hasAction("favourite");useFavourite&&(html+='\x3cspan class\x3d"ico live-event-bind-add '+(store.app.isFavourite?
"ico-wish-min-selected":"ico-wish-min")+'" element-id\x3d"'+store.id+'"\x3e\x3c/span\x3e');html+='\x3cspan class\x3d"store-name"\x3e'+inditex.text("bershka")+" - "+store.city+"\x3c/span\x3e\x3cbr/\x3e";store.addressLines[0]!=store.name&&(html+=store.name+"\x3cbr/\x3e");html+=store.addressLines[0]+"\x3cbr/\x3e"+store.zipCode+" - "+store.city+" - "+store.countryName+"\x3cbr/\x3e"+store.phones[0]+"\x3cbr/\x3e"+inditex.getSections(store.sections);html+="\x3c/div\x3e";useFavourite&&(html+='\x3cspan class\x3d"change-store live-event-bind-favourite" element-id\x3d"'+
store.id+'"\x3e'+inditex.text(store.app.isFavourite?"storelocator.delFav":"storelocator.addFav")+"\x3c/span\x3e");this.hasAction("select")&&(html+='\x3cspan class\x3d"change-store live-event-bind-select" element-id\x3d"'+store.id+'"\x3e'+inditex.text("storeLocator.selectStore")+"\x3c/span\x3e");return html+="\x3c/div\x3e"}return""};
bskMapCtrl.prototype.getDroppointMarkerHtml=function(point){if(point){var useFavourite=inditex.iStoreJSON.details.showFavouriteStores&&"G"!==inditex.iUserJSON.userType&&this.hasAction("favourite"),html="\x3cdiv class\x3d'droppoint-info-container "+(this.hasAction("select")?"live-event-bind-select":"")+"'\x3e\x3cdiv class\x3d'droppoint-info'\x3e";useFavourite&&(html+='\x3cspan class\x3d"ico live-event-bind-add '+(point.app.isFavourite?"ico-wish-min-selected":"ico-wish-min")+'" element-id\x3d"'+point.id+
'"\x3e');html+='\x3cspan class\x3d"droppoint-name"\x3e'+point.city+" - "+point.name+"\x3c/span\x3e\x3cbr/\x3e"+point.street+(point.homeNo?" - "+point.homeNo:"")+"\x3cbr /\x3e"+point.zipCode+" - "+point.city+(point.countryName?" - "+point.countryName:"")+"\x3cbr/\x3e";point.telephone&&(html+=point.telephone+"");html+="\x3c/div\x3e";useFavourite&&(html+='\x3cspan class\x3d"change-droppoint live-event-bind-favourite" element-id\x3d"'+point.id+'"\x3e'+inditex.text(store.app.isFavourite?"storelocator.delFav":
"storelocator.addFav")+"\x3c/span\x3e");this.hasAction("select")&&(html+="\x3cspan class\x3d'change-droppoint live-event-bind-select' element-id\x3d'"+point.id+"'\x3e"+inditex.text("storeLocator.selectPickPoint")+"\x3c/span\x3e");return html+="\x3c/div\x3e"}return""};bskMapCtrl.prototype.getMarkerHtml=function(element){return"store"==this.type?this.getStoreMarkerHtml(element):this.getDroppointMarkerHtml(element)};
bskMapCtrl.prototype.findElementById=function(elementId){for(var i=0,len=this.elements.length;i<len;i++){var element=this.elements[i];if(element.id==elementId)return element}return null};
bskMapCtrl.prototype.execAction=function(elementId,action){var element=this.findElementById(elementId),latitude,longitude,bounds;if(this.map){var center=this.map.iMap.getCenter();latitude=center.lat();longitude=center.lng();bounds=this.map.iMap.getBounds()}if(this.onAction)this.onAction({store:element,droppoint:element,point:element,element:element,action:action,latitude:latitude,longitude:longitude,bounds:bounds})};
bskMapCtrl.prototype.focusElement=function(element){for(var i=0,len=this.elements.length;i<len;i++){var current=this.elements[i];if(current.id==element.id){this.map.iMap.setZoom(13);this.map.focusMark(i);break}}};bskMapCtrl.prototype.closeWindows=function(){for(var i=0,len=this.map.iWindows.length;i<len;i++)this.map.iWindows[i].close()};bskMapCtrl.prototype.blurElement=function(){this.closeWindows();this.map.focus()};
