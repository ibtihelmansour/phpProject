(function(){function sort(item1,item2){var count1=item1&&1<item1.length&&item1[1]?(item1[1].match(/\{/g)||[]).length:0,count2=item2&&1<item2.length&&item2[1]?(item2[1].match(/\{/g)||[]).length:0;return count1===count2&&void 0!==item1[2]&&void 0!==item2[2]?item1[2]-item2[2]:count1-count2}function toPageId(value){return"Itx"+value.replace(/^[a-z]|-[a-z]/g,function(v){return v.toUpperCase()}).replace(RegExp("-","g"),"")+"Page"}function expand(action){var key,item,list0,list1,t;list0=[];list1=[];var map=
