function lookBookProductCtrl(config,jetloreTracking,trackingService,shopCartService){this.config=config;this.jetloreTracking=jetloreTracking;this.trackingService=trackingService;this.shopCartService=shopCartService;this.init.apply(this,arguments)}lookBookProductCtrl.prototype.init=function(){};
lookBookProductCtrl.prototype.$onInit=function(){var self=this;this.imageType=this.imageType||"BUNDLE_COMPONENT";this.imageSize=this.imageSize||"LARGE";this.config.get(["SEASONS_BACK_COMING_NOT_ALLOWED"]).then(function(values){var refSplitted=self.product.reference.split("-"),disabledBackSoonComingSoonSeasons=values[0]?values[0].replace(/\s/g,"").split(","):[],productSeason=refSplitted&&1<refSplitted.length?refSplitted[1]:null;self.isBackSoonCommingSoonDisabled=!inditex.iStoreJSON.details.stockNotification||
-1!=disabledBackSoonComingSoonSeasons.indexOf(productSeason)});this.isOpenForSale=inditex.isOpenForSale();this.hashTags=inditex.getHashTags(this.product);this.selectColor(this.product.colors[0]);this.jetloreTracking.view(this.product,this.selectedColor)};
lookBookProductCtrl.prototype.getSizeClass=function(size){var clazz;this.isOpenForSale?"in_stock"==size.stock?clazz=this.selectedSize==size?"active":"selectable":(clazz="unavailable",this.isBackSoonCommingSoonDisabled||"back_soon"!=size.stock&&"comming_soon"!=size.stock||(clazz+=" notifyMe")):clazz="disabled";return clazz};
lookBookProductCtrl.prototype.selectSize=function(size){this.priceZero=!1;switch(size.stock){case "in_stock":this.selectedSize=size;this.priceZero=!size.price;break;case "back_soon":case "comming_soon":inditex.iStoreJSON.details.stockNotification&&this.openEmailMeModal(size)}};
lookBookProductCtrl.prototype.selectColor=function(color){this.selectedColor=color;this.selectedSize=null;this.selectedColor?(this.priceZero=!this.selectedColor.price||!this.selectedColor.price.minPrice&&!this.selectedColor.price.maxPrice,this.iRangePrice=this.selectedColor.price):(this.priceZero=!this.product.price||0==this.product.price.minPrice&&0==this.product.price.maxPrice,this.iRangePrice=this.product.price);this.sProductImageUrl=this.selectedColor.image[this.imageType][this.imageSize][0].src};
lookBookProductCtrl.prototype.openEmailMeModal=function(size){var url=inditex.encodeUrl({action:"ItxStockNotificationPage",params:{storeId:inditex.iStoreId,langId:inditex.iLangId,catalogId:inditex.iCatalogId,categoryId:inditex.iCategoryId,type:size.stock},isAjax:!1});inditex.xRefreshPopupUrl(url,675,"auto",function(){inditex.initModalController("itxStockNotificationPage",{product:self.product,size:size,imageUrl:self.selectedColor.images.BUNDLE_COMPONENT.LARGE[0],color:self.selectedColor})},null,"modal-qs")};
lookBookProductCtrl.prototype.addToCart=function(){var self=this,partNumber=this.selectedSize.partnumber.substr(0,11),item={sku:this.selectedSize.sku,parentId:this.product.id,name:this.product.name,color:this.selectedColor.name,colorId:this.selectedColor.id,quantity:1,unitComparePrice:this.selectedSize.oldPrice,unitPrice:this.selectedSize.price,image:this.selectedColor.originalColor&&this.selectedColor.originalColor.image,reference:this.product.reference,productType:this.product.productType,size:this.selectedSize.name,
jetLoreId:partNumber,productId:this.product.id};this.shopCartService.addItem(item);this.trackingService.trackEvent({name:"add-to-cart",data:{product:this.product,selectedColor:this.selectedColor,productSize:this.selectedSize.name,quantity:1,cf:"checkout",eventCategory:"lookbook_vista_rapida_look",action:"anadir_producto_cesta",label:inditex.iCategoryId+"_"+this.product.id,list:"lookbook_"+inditex.iCategoryId}})};
lookBookProductCtrl.prototype.goToProductPage=function(){this.trackingService.trackEvent({name:"product-click",data:{product:this.product,selectedColor:this.selectedColor,category:inditex.iCategoryId,list:"lookbook",position:this.index+1,cf:"catalogo",eventCategory:"lookbook_vista_rapida_look",action:"ver_producto",label:inditex.iCategoryId+"_"+this.product.reference}})};
angular.module("itxApp").component("addressForm",{templateUrl:"tplAddressForm",controller:["userAddressesFactory","daumService","orderFactory","checkoutShippingState","droppointsFactory","bskConfigurationFactory","$timeout","$rootScope","bskCountriesConstants","WorldWideHelper","ItxExecuteCache","bskCountries",addressFormCtrl]});
function addressFormCtrl(userAddresses,daumService,order,shippingState,droppointsFactory,config,$timeout,$rootScope,countriesConstants,wwHelper,executeCache,bskCountries){this.userAddresses=userAddresses;this.daumService=daumService;this.order=order;this.config=config;this.shippingState=shippingState;this.droppointsFactory=droppointsFactory;this.countriesConstants=countriesConstants;this.wwHelper=wwHelper;this.bskCountries=bskCountries;this.executeCache=executeCache;this.$timeout=$timeout;this.$rootScope=
$rootScope;this.addressMaxLength=inditex.xGetMaxLength();this.addressMaxLength2=inditex.xGetMaxLength2();this.init.apply(this,arguments)}
addressFormCtrl.prototype.init=function(){this.countryCode=inditex.iStoreJSON.countryCode.toUpperCase();this.detailedPrivacyPolicy="1"==bershka.getStoreConfiguration("DETAILED_PRIVACY_POLICY");var self=this;this.config.get("ENABLE_PEC_AND_RECEIVER_CODE").then(function(values){self.xconfEnablePEC=values&&(1==values[0]||"true"==values[0]);self.enablePEC=self.xconfEnablePEC&&"IT"==self.shippingState.addresses.billing.countryCode})};
addressFormCtrl.prototype.translate=function(){return inditex.text.apply(inditex,arguments)};addressFormCtrl.prototype.searchDaum=function(form){var dest=this.shippingState[form];dest&&this.daumService.promise().then(function(data){data&&(dest.addressLines=[data.address1,data.address2],dest.zipCode=data.postcode,dest.city=data.city)})};
addressFormCtrl.prototype.showNIF=function(){var countryCode=inditex.iStoreJSON.countryCode.toUpperCase();return"GB"==countryCode||"IE"==countryCode?this.shippingState.addresses.billing.isCompany:!0};
addressFormCtrl.prototype.shippingCountryChanged=function(countryCode){if(this.bskCountries.isInWW()){this.wwHelper.setISO(countryCode);var self=this;this.executeCache.execute([{action:"catalog:store",params:{storeId:inditex.iStoreId,languageId:inditex.iLangId,countryISO:countryCode},force:!0}]).then(function(results){inditex.iStoreJSON=results[0].value;self.bskCountries.init();self.shippingState.addresses.canEditBilling&&!self.shippingState.addresses.isSameAddress&&(self.shippingState.addresses.billing.countryCode=
inditex.iStoreJSON.countryCode,self.shippingState.addresses.billing.countryName=inditex.iStoreJSON.countryName)})}};
angular.module("itxApp").component("checkoutSummary",{templateUrl:"tplCheckoutSummary",controller:["$rootScope","$timeout","$element","orderFactory","checkoutShippingState","paymentMethodsFactory","shopCartService","shippingMethodsFactory","bskDeviceService","trackingService","giftCardsFactory","storesFactory","droppointsFactory","googleBaiduHelper","bskGDPRHelper","bskStateFactory","orderConstants","unifiedStockConstants","bskCountries",checkoutSummaryCtrl]});
function checkoutSummaryCtrl($rootScope,$timeout,$element,orderFactory,shippingState,paymentMethods,shopCartService,shippingMethods,deviceService,trackingService,giftCards,storesFactory,droppointsFactory,googleBaiduHelper,GDPRHelper,stateFactory,orderConstants,unifiedStock,bskCountries){this.droppointsFactory=droppointsFactory;this.storesFactory=storesFactory;this.order=orderFactory;this.shippingState=shippingState;this.shopCartService=shopCartService;this.shippingMethods=shippingMethods;this.deviceService=
deviceService;this.trackingService=trackingService;this.paymentMethods=paymentMethods;this.giftCards=giftCards;this.googleBaiduHelper=googleBaiduHelper;this.stateFactory=stateFactory;this.constants=orderConstants;this.unifiedStock=unifiedStock;this.shippingState=shippingState;this.bskCountries=bskCountries;this.$timeout=$timeout;this.$rootScope=$rootScope;this.$element=$element;this.GDPRHelper=GDPRHelper;this.hasGDPRprivacy=GDPRHelper.hasDocuments("checkout-privacy");this.hasGDPRconditions=GDPRHelper.hasDocuments("checkout-conditions");
this.hasGDPRwithdrawl=GDPRHelper.hasDocuments("checkout-withdrawl");this.hasGDPRshipping=this.GDPRHelper.hasDocuments("checkout-shipping");this.init.apply(this,arguments)}
checkoutSummaryCtrl.prototype.init=function(){var self=this;this.$rootScope.$on(bershka.events.AddressForm,function(ev,data){"shipping"==self.order.step?self.toPayment=data:self.toFinish=data&&self.paymentMethods.selected&&"gift_card"==self.paymentMethods.selected.app.kind&&0<self.giftCards.pendingAmount?!1:data});this.deviceService.addObserverChange(function(oldType,newType){"mobile"==newType?self.$element.trigger("sticky_kit:detach"):self.$timeout(function(){self.$element.stick_in_parent({offset_top:bershka.getHeaderHeight()+
20})},0,!1)},!0);this.hasGift=!1;var shopCart=this.shopCartService.getShopCart();shopCart&&!this.shopCartService.hasVirtualItems()&&(this.hasGift=!(!shopCart.giftPacking&&1!=shopCart.giftTicket&&!shopCart.message));this.hasPromo=null!=this.shopCartService.getCurrentPromo();this.availableGift=1==inditex.iStoreJSON.details.showGiftTicket||1==inditex.iStoreJSON.details.packingGift;("mobile"==this.deviceService.getActualState()&&bershka.config("ShowItemsOnLoadMobile")||"mobile"!=this.deviceService.getActualState()&&
bershka.config("ShowItemsOnLoadDesktop"))&&this.toggleElement(".summary-products",!0);this.hasWithdrawal=1==bershka.config("extendedCheckoutLegalDocuments")};checkoutSummaryCtrl.prototype.$doCheck=function(){!this.paymentMethods.selected&&this.toFinish&&(this.toFinish=!1)};checkoutSummaryCtrl.prototype.canFinish=function(){return"payment"!=this.order.step||0!=this.order.cart.totalAmount&&0!=this.giftCards.pendingAmount?this.toFinish:!0};
checkoutSummaryCtrl.prototype.canContinue=function(){if(!this.shippingState.methods.selected||this.shopCartService.hasIndomError()&&this.shopCartService.getIndomError()!=this.unifiedStock.errors.WARN_RESTRICTED_METHODS)return!1;if(this.shippingState.methods.selected.app.cronosEnabled&&this.shippingState.step==this.constants.shipping.steps.SPECIFIC_SELECT)return!!this.shippingState.methods.specific;if(this.shippingState.step==this.constants.shipping.steps.METHOD_SELECT)return!!this.shippingState.methods.selected;
var selected=this.shippingState.methods.selected;switch(selected.app.kind){case "droppoint":return this.toPayment&&this.droppointsFactory.selected;case "pickup":return this.toPayment&&this.storesFactory.selected;case "delivery":if("definedDeliveryDate"!=selected.kind)return this.toPayment||this.shippingState.addresses.useInstant}return this.toPayment};
checkoutSummaryCtrl.prototype.continueShipping=function(){switch(this.shippingState.step){case this.constants.shipping.steps.METHOD_SELECT:this.shippingState.addresses.instant&&this.shippingState.methods.selected.app.cronosEnabled?this.order.showShippingTypes():this.order.showAddressForm();break;case this.constants.shipping.steps.ADDRESS_FORM:this.order.showShippingTypes()}};checkoutSummaryCtrl.prototype.finishShipping=function(){this.order.commitShipping()};
checkoutSummaryCtrl.prototype.commitShipping=function(){var self=this,AddressSuggestion=1==bershka.getStoreConfiguration("AddressSuggestion");this.suggestedAddress=null;AddressSuggestion&&!this.shippingState.addressValidated&&"delivery"==this.shippingState.methods.selected.app.kind?self.googleBaiduHelper.getSuggestions(self.shippingState.addresses.shipping).then(function(res){self.compareSuggestions(res);self.suggestedAddress?self.$timeout(function(){inditex.xOpenAddressValidator(400,600,{suggestedAddress:self.suggestedAddress,
shipping:self.shippingState.addresses.shipping})},0):self.order.commitShipping()},function(){self.order.commitShipping()}):this.order.commitShipping()};
checkoutSummaryCtrl.prototype.compareSuggestions=function(results){this.suggestedAddress=null;if(results&&results.address_components)for(var i=0;i<results.address_components.length;i++){var component=results.address_components[i];-1!=component.types.indexOf("postal_code")&&this.shippingState.addresses.shipping.zipCode!=component.long_name&&(this.suggestedAddress=JSON.parse(JSON.stringify(this.shippingState.addresses.shipping)),this.suggestedAddress.zipCode=component.long_name)}};
checkoutSummaryCtrl.prototype.finishPayment=function(){this.order.commitPayment()};checkoutSummaryCtrl.prototype.openTermsAndConditions=function(){this.hasGDPRconditions?this.GDPRHelper.showComplete("checkout-conditions"):inditex.openTermsAndConditions()};checkoutSummaryCtrl.prototype.openPrivacyPolicy=function(){"shipping"==this.order.step&&this.hasGDPRshipping?this.GDPRHelper.showSummary("checkout-shipping",!0):this.hasGDPRprivacy?this.GDPRHelper.showComplete("checkout-privacy"):inditex.openPrivacyPolicy()};
checkoutSummaryCtrl.prototype.checkShippingPrivacyPolicy=function(){this.order.shippingPrivacyPolicy&&this.hasGDPRshipping&&this.GDPRHelper.showSummary("checkout-shipping")};checkoutSummaryCtrl.prototype.openWithdrawal=function(){this.hasGDPRwithdrawl?this.GDPRHelper.showComplete("checkout-withdrawl"):inditex.openWithdrawal()};checkoutSummaryCtrl.prototype.editCart=function(where){this.trackingService.trackEvent({name:"go-to-cart",data:{currentStep:this.order.step,section:where}});inditex.goToShopCart(where)};
checkoutSummaryCtrl.prototype.getTotalGiftCard=function(){var adjustment=0;if(this.giftCards.cards)for(var i=0,len=this.giftCards.cards.length;i<len;i++)adjustment+=parseInt(this.giftCards.cards[i].amount,10);return adjustment};
checkoutSummaryCtrl.prototype.getDynamicPriceWithShipping=function(){var ret=this.shopCartService.getTotalProductsPrice()+this.shopCartService.getTotalAdjustmentsPriceNoShipping();this.shippingState.methods.selected&&!this.shippingState.methods.selected.app.cronosEnabled?ret+=this.shippingState.methods.selected.priceWithDiscounts:this.shippingState.methods.specific&&(ret+=this.shippingState.methods.specific.priceWithDiscounts);return ret};
checkoutSummaryCtrl.prototype.toggle=function($event){$event&&($event.stopImmediatePropagation(),$event.preventDefault());var $summary=$(".summary-collapsed");$summary.hasClass("visible")?($summary.removeClass("visible"),bershka.showScrolls()):($summary.addClass("visible"),bershka.hideScrolls())};
checkoutSummaryCtrl.prototype.toggleElement=function(parent,instant){var $container=$(parent);$container.toggleClass("opened");instant?($container.find(".toggle-content").show(),$(document.body).trigger("sticky_kit:recalc")):$container.find(".toggle-content").slideToggle(400,function(){$(document.body).trigger("sticky_kit:recalc")})};
checkoutSummaryCtrl.prototype.getPrice=function(){if("shipping"==this.order.step)return this.getDynamicPriceWithShipping()-this.getTotalGiftCard();if("payment"==this.order.step){var taxes=0;inditex.iStoreJSON.details.isTaxIncluded||(taxes=this.shopCartService.getTotalTaxPrice());return this.shopCartService.getTotalPrice()-this.getTotalGiftCard()-taxes}};
angular.module("itxApp").component("userAddress",{bindings:{address:"\x3c"},templateUrl:"tplUserAddress",controller:["userAddressesFactory","$attrs","addressHelperFactory",userAddressCtrl]});function userAddressCtrl(userAddresses,$attrs,addressHelperFactory){this.userAddresses=userAddresses;this.addressHelper=addressHelperFactory;this.setFromAttrs($attrs);this.init.apply(this,arguments)}
userAddressCtrl.prototype.setFromAttrs=function($attrs){for(var attrs="showName showEmail showAddress showCity showCountry showPhone".split(" "),value=!1,i=0,len=attrs.length;i<len;i++){var attr=attrs[i];$attrs.hasOwnProperty(attr)&&(value=this[attr]=!0)}if(!value)for(i=0,len=attrs.length;i<len;i++)attr=attrs[i],this[attr]=!0};userAddressCtrl.prototype.init=function(){};userAddressCtrl.prototype.setFromSplitted=function(value){if(value){var ret=value.split("#");return ret[ret.length-1]}};
userAddressCtrl.prototype.$onChanges=function(changes){if(this.address){this.address=this.addressHelper.clean(this.address);this.city=this.setFromSplitted(this.address.city);this.district=this.setFromSplitted(this.address.colony);"-"==this.district&&(this.district=null);var self=this;this.userAddresses.getStates().then(function(states){for(var i=0,len=states.length;i<len;i++){var state=states[i];if(state.code==self.address.stateCode){self.province=state;break}}})}};
