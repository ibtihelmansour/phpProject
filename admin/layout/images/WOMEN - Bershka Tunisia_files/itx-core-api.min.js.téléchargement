self.iScope.iSearchPointsAddress}"FR"==inditex.iStoreJSON.countryCode&&(action.params.postalCode?action.params.city=self.iScope.iSearchPointsCity:action.params.postalCode=self.iScope.iSearchPointsCity);actions.push(action);inditex.execute({actions:actions,onFinish:function(status,points){1==status?(self.iScope.iSearchedPoints=points.dropPoints,0<points.dropPoints.length?($.each(points.dropPoints,function(i,point$$0){isBaiduActive?inditex.translateGoogleToBaiduCoords(point$$0.longitude,point$$0.latitude,
