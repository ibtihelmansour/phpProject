relatedProductCtrl.prototype._prepareColors=function(){var product=this.product;if(void 0==product.selectedColorId)this.iColor=product.colors[0];else for(var i=0,len=product.colors.length;i<len;i++){var color=product.colors[i];if(product.selectedColorId==color.id){this.iColor=color;break}}this.iRange=this.iColor?this.iColor.price:this.product.price;var selectedColorId=this.iColor?this.iColor.id:null;if(this.selectedColorId!=selectedColorId){this.selectedColorId=selectedColorId;this.sProductUrl=this._getProductDetailURL();
