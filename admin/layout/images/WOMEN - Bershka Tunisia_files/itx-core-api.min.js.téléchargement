categoryDetailCtrl.prototype.getNumberResultsText=function(instance){var numResults=instance.numberVisibleProducts(),text=numResults+" ";return text=0==numResults||1<numResults?text+inditex.text("categoryPage.filter.multiArticle"):text+inditex.text("categoryPage.filter.singleArticle")};
categoryDetailCtrl.prototype.prepareGrid=function(){function prepareGridHelper(child,index){var instance=self.gridOrchestrator.getInstance("subGrid_"+index);instance.title=child.name;instance.description=child.description;instance.filter=function(product){for(var relatedCategories=product.relatedCategories||[],i=0,len=product.relatedCategories.length;i<len;i++)if(product.relatedCategories[i].id==child.id)return!0;return!1};return instance}this.gridOrchestrator.clear();var self=this;this.subgrids=
[];if(this.categoryFactory.oCategory._config.isMultiCategory&&this.categoryFactory.oCategory._children.length)for(var i$$0=0,len$$0=this.categoryFactory.oCategory._children.length;i$$0<len$$0;i$$0++){var child$$0=this.categoryFactory.oCategory._children[i$$0];child$$0._config&&!child$$0._config.isHidden&&this.subgrids.push(prepareGridHelper(child$$0,i$$0))}this.baseGrid=prepareGridHelper({},"master");this.baseGrid.filter=function(){return!0}};
categoryDetailCtrl.prototype.init=function(){this.sortOpened=!1;this.hasValidCategory=this.showSubgrids=this.showSeparators=!0;this._bindEvents();this.config.get("ALTERNATIVE_CURRENCY").then(function(value){if(null!==value)try{inditex.alternativeCurrency=JSON.parse(value)}catch(e){}})};categoryDetailCtrl.prototype.setLoading=function(state){(this.loadingGrid=state)?$("#loadingGrid").show():this.$timeout(function(){$("#loadingGrid").hide()},0,!1)};
categoryDetailCtrl.prototype.loadFullCategory=function(category){var self=this;category=category||this.categoryFactory.oCategory;this.filtersOpened=this._getFiltersDefaultVisibility();this.setLoading(!0);this.RTBHouseService.categoryEvent(category._config.id);this.nextCategory=this.categoryFactory.getNextCategory();this.prevCategory=this.categoryFactory.getPreviousCategory();var stock$$0=this.executeCache.execute([{action:"catalog:categoryStock",params:{storeId:inditex.iStoreId,languageId:inditex.iLangId,
catalogId:inditex.iCatalogId,categoryId:category._config.linkCategoryId}}]).then(function(result){return result[0].value.stocks}),listing=this.executeCache.execute([{action:"catalog:products",params:{storeId:inditex.iStoreId,languageId:inditex.iLangId,catalogId:inditex.iCatalogId,categoryId:category._config.linkCategoryId}}]).then(function(results){return results[0].value.products}),first;category._config.jetloreSorting&&(first=this.$q.race(stock$$0,listing).then(function(result){return self.jetloreRanking.sortCategory(category,
result.length).catch(function(){})}));this.$q.all([stock$$0,listing,first]).then(function(results){var stock=results[0],products=results[1],order=results[2];if(products.length)return self._prepareViewProduct(products,stock,order).then(function(){if(category._config.isFilterCategory){var filter=self.gridFilterService.searchFilter("tipology",category._config.id);filter&&self.gridFilterService.activate("tipology",filter)}})},function(){inditex.xOpenErrorPopUp(360,"auto")}).finally(function(){self.setLoading(!1)})};
categoryDetailCtrl.prototype.loadSimpleCategory=function(category){this.gridFilterService.refreshFromState();this.gridSortService.refreshFromState();this.gridOrchestrator.refresh();this.setLoading(!1);this._waitAndRecalc();inditex.iXCategoryLength=this.gridOrchestrator.numberProducts()};
categoryDetailCtrl.prototype._prepareViewProduct=function(productList,stock,order){var self=this;return this.productNormalizerService.normalize(productList,stock).then(function(products){self._sortGridProducts(products,order)})};
categoryDetailCtrl.prototype._sortReorderProducts=function(products,ordered){var finalProducts=[],restOfProducts=products;if(ordered&&ordered.length){this.$log.log("categoryDetailCtrl","Sorting received: %O",ordered);for(var i=0,len=ordered.length;i<len;i++){for(var tempRestProducts=[],j=0,numProducts=restOfProducts.length;j<numProducts;j++){var product=restOfProducts[j],isComingSoon="back_soon"==product.stock||"coming_soon"==product.stock;isComingSoon||product.jetloreId!=ordered[i]?tempRestProducts.push(product):
(product.isJetloreSorted=!0,finalProducts.push(product))}restOfProducts=tempRestProducts}if(restOfProducts.length)for(this.$log.log("categoryDetailCtrl","Adding not sortable items into position: %O",restOfProducts),i=0,len=restOfProducts.length;i<len;i++){var product=restOfProducts[i],index=products.indexOf(product);-1!=index?finalProducts.splice(index,0,product):finalProducts.push(product)}if(this.categoryConstants.jetloreCSBSReorder){for(var csbsProducts=[],restOfProducts=[],i=0,len=finalProducts.length;i<
len;i++)product=finalProducts[i],"coming_soon"==product.stock||"back_soon"==product.stock?csbsProducts.push(product):restOfProducts.push(product);this.$log.log("categoryDetailCtrl","Moving %d coming soon and back soon at the back",csbsProducts.length);finalProducts=restOfProducts.concat(csbsProducts)}}else finalProducts=products;return finalProducts};
categoryDetailCtrl.prototype._cleanProducts=function(products){var config=this.categoryFactory.oCategory&&this.categoryFactory.oCategory._config;this.allProductsBanners=0<products.length;if(config&&config.showOnlyPromotions){for(var cleaned=[],i=0,len=products.length;i<len;i++){var product=products[i],price=product.colors&&product.colors[0]?product.colors[0].price:product.price;if(product.isBanner||price&&price.discount)cleaned.push(product),product.isBanner||(this.allProductsBanners=!1)}return cleaned}i=
0;for(len=products.length;i<len;i++)if(!products[i].isBanner){this.allProductsBanners=!1;break}return products};
categoryDetailCtrl.prototype._sortGridProducts=function(products,ordered){var finalProducts=this._cleanProducts(products),finalProducts=this._sortReorderProducts(finalProducts,ordered);this.isSpecialCategory=this.categoryFactory.oCategory._config&&this.categoryFactory.oCategory._config.isSpecialCategory;$("html").removeClass("all-product-banners is-special-category");this.isSpecialCategory?$("html").addClass("is-special-category"):this.allProductsBanners&&$("html").addClass("all-product-banners");
this.aProducts=finalProducts;this.prepareGrid();this.gridOrchestrator.setProducts(finalProducts);inditex.iXCategoryLength=this.gridOrchestrator.numberProducts();if(this.gridChatEnabled=finalProducts.length>this.categoryConstants.gridChatLimit)$(window).off("scroll.gridchat"),$(window).on("scroll.gridchat",_.debounce(function(){var $elem=$("#fixed-chat"),offset=$elem.offset();offset&&offset.top>$(document).height()/2?($elem.addClass("visible"),$("bsk-top").addClass("chatVisible")):($elem.removeClass("visible"),
$("bsk-top").removeClass("chatVisible"))},50));this.setLoading(!1)};categoryDetailCtrl.prototype.$onDestroy=function(){if(this.onDestroyCallbacks)for(var i=0,len=this.onDestroyCallbacks.length;i<len;i++)this.onDestroyCallbacks[i]();this.onDestroyCallbacks=void 0;$(document).off(bershka.events.AddToWishlist+".category-detail")};
