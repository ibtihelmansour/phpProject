inditex.xCheckNoDesktop=function(categoryId,key,element){$(element).addClass("is-mobile")};inditex.xCheckNoWeb=function(categoryId,key,element){$(element).addClass("hidden")};inditex.xCheckNew=function(categoryId,key,element){$(element).addClass("blink-category-label new-category")};inditex.xCheckWow=function(categoryId,key,element){$(element).addClass("blink-category-label wow-category")};inditex.xCheckHelen=function(categoryId,key,element){$(element).addClass("blink-category-label helen-category")};
