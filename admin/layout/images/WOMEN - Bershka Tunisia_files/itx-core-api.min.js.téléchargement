(function(){var Mochat=Mochat||{Datatypes:{Enumerations:{}}};window.Mochat=Mochat;Mochat.normalizeJid=function(jid){var zaraDomain="zara.com";if(!jid){var min=1E14,max=1E15,randomJid=Math.floor(Math.random()*(max-min))+min;return randomJid.toString(10)+"@"+zaraDomain}var node=Strophe.getNodeFromJid(jid),domain=Strophe.getDomainFromJid(jid),resource=Strophe.getResourceFromJid(jid),zaraJid;if(node)zaraJid=node,zaraJid=zaraJid+"@"+(domain!==zaraDomain?zaraDomain:domain);else{if(isNaN(domain))throw new TypeError("Cannot compose a normalized jid with specified jid "+
jid);zaraJid=domain+"@"+zaraDomain}return zaraJid+=resource?"/"+resource:""};Mochat.passesLuhn=function(arr){return function(ccNum){if(/[^0-9-\s]+/.test(ccNum))return!1;for(var len=ccNum.length,bit=1,sum=0,val;len;)val=parseInt(ccNum.charAt(--len),10),sum+=(bit^=1)?arr[val]:val;return sum&&0===sum%10}}([0,2,4,6,8,1,3,5,7,9]);Mochat.maskCardNumber=function(cardNumber){return cardNumber.replace(/[\d-\s]+(?=\d{4})/,"************")};Mochat.maskCreditCards=function(str){var re=/\b(?:\d[ -]*?){13,16}/g,
m,strToClean=str;do if(m=re.exec(strToClean)){var match=m[0].replace(/[-\s]+/gm,"");if(Mochat.passesLuhn(match))var replaceRE=new RegExp(m[0],"g"),strToClean=strToClean.replace(replaceRE,Mochat.maskCardNumber(match))}while(m);return strToClean};Mochat.Datatypes.Enumerations.os=Nomasystems.ChatCore.Util.deepFreeze({iOS:"iOS",android:"android"});Mochat.Datatypes.Enumerations.businessType=Nomasystems.ChatCore.Util.deepFreeze({online:1,bam:2});Mochat.Datatypes.Enumerations.fromType=Nomasystems.ChatCore.Util.deepFreeze({contact:"contact",
