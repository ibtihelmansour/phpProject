(function(){function queryStringDivider(paramName,params){var paramsMaxLength=1800,paramsLength=0,paramNameLength=paramName.length+2,indexesToSplit=[],queryStrings=[],myParams=params;for(i=0;i<params.length;i++)paramsLength+=paramNameLength,paramsLength+=params[i].length,paramsLength>paramsMaxLength&&(indexesToSplit.push(i),paramsLength=0),i==params.length-1&&indexesToSplit.push(i+1);for(i=0;i<indexesToSplit.length;i++){var queryString=[],start=void 0!=indexesToSplit[i-1]?indexesToSplit[i-1]:0;for(j=
start;j<indexesToSplit[i];j++)queryString.push(myParams.slice(j,j+1)[0]);queryStrings.push(queryString)}return queryStrings}var iXconfMap,iXconfStatus=0,iResponseStatus=1;inditex.getXconfiguracionValue=function(key,callback){if(0==iXconfStatus&&void 0!=inditex.iXconfVars&&0<inditex.iXconfVars.length){inditex.iXconfVars.sort();iXconfMap={};iXconfStatus=1;var queryStrings=queryStringDivider("variable",inditex.iXconfVars),requestCount=0;for(i=0;i<queryStrings.length;i++){var action={action:"oam:xconfiguracion",
params:{storeId:inditex.iStoreId,languageId:inditex.iLangId,variable:queryStrings[i]}};inditex.execute({actions:[action],onFinish:function(status,response){requestCount++;if(1==status&&response&&null!=response){if(response instanceof Array)for(i=0;i<response.length;i++){var myKey=response[i].key;iXconfMap[myKey]=response[i].value}else iXconfMap[response.key]=response.value;"function"===typeof callback&&requestCount==queryStrings.length&&(iXconfStatus=0==iResponseStatus?0:2,callback(iResponseStatus,
