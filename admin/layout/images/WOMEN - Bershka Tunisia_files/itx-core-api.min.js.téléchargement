1200)});$(window).resize(function(){self._updateReadOn();setTimeout(function(){jQuery.waypoints("refresh")},1200)});self._updateReadOn()};
ItxShoppingGuidePageClass.prototype._updateReadOn=function(elem){if(elem)if(elem.parent().hasClass("collapsed")){if(0<elem.find(".readon-start").length){var article_top=elem.offset().top,readon_start=elem.find(".readon-start").offset().top,max_height=readon_start-article_top;elem.css("height",max_height+"px");setTimeout(function(){$("body, html").animate({scrollTop:elem.offset().top},"slow")},1200)}}else{if(0<elem.find(".readon-end").length){var article_top=elem.offset().top,readon_end=elem.find(".readon-end").offset().top,
max_height=readon_end-article_top;elem.css("height",max_height+"px")}}else $(".article-content").each(function(){if($(this).parent().hasClass("collapsed")){if(0<$(this).find(".readon-start").length){var article_top=$(this).offset().top,readon_start=$(this).find(".readon-start").offset().top,max_height=readon_start-article_top;$(this).css("height",max_height+"px")}}else if(0<$(this).find(".readon-end").length){var article_top=$(this).offset().top,readon_end=$(this).find(".readon-end").offset().top,
max_height=readon_end-article_top;$(this).css("height",max_height+"px")}})};ItxShoppingGuidePageClass.prototype._toggleMessage=function(elem){var textField=elem.find("span.showmore-text"),actual=textField.html(),newField=textField.attr("altMessage");textField.html(newField);textField.attr("altMessage",actual)};
