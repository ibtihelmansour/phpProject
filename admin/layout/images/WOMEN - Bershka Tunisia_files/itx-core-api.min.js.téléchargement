ItxAjaxDeliveryPointPageClass.prototype._search=function(){var self=this,getCoords=null,isBaiduActive=!1;"cn"===inditex.iCountryCode.toLowerCase()&&inditex.iBaiduMapsKey?(isBaiduActive=!0,getCoords=inditex.getBaiduCoords):getCoords=inditex.getGoogleCoords;if($("#ajaxDropPointLocatorForm").valid()){var callback=function(result){if(result){var actions=[],action={action:"bam:dropPoint",params:{storeId:inditex.iStoreId,languageId:inditex.iLangId,countryCode:inditex.iStoreJSON.countryCode}};if("ES"==inditex.iStoreJSON.countryCode)action.params.latitude=
