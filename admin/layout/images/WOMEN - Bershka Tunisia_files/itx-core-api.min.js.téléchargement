bskPromotionsCtrl.prototype.getComparablePrice=function(shopCart,descriptor){for(var currencyDecimals=Math.pow(10,inditex.iStoreJSON.details.locale.currencyDecimals),value=shopCart.totalProduct,done=[],i=0,len=descriptor.exceptions.products.length;i<len;i++){var id=descriptor.exceptions.products[i];if(-1==done.indexOf(id)){done.push(id);for(var j=0,len=shopCart.items.length;j<len;j++){var item=shopCart.items[j];item.parentId==id&&-1==done.indexOf(id)&&(value-=item.unitPrice*item.quantity)}}}return 0>
value?0:value*currencyDecimals};
bskPromotionsCtrl.prototype.shopCartCheckPromotion=function(shopCart,descriptor){var ret={reached:!1,left:!1,persistent:descriptor.persistent,text:""},currencyDecimals=Math.pow(10,inditex.iStoreJSON.details.locale.currencyDecimals);if(descriptor&&(-1!=descriptor.location.indexOf(this.location)||-1!=descriptor.location.indexOf("all"))){var value;switch(descriptor.type){case "items":for(var i=value=0,len=shopCart.items.length;i<len;i++)value+=shopCart.items[i].quantity;break;case "products":value=shopCart.items.length;
default:value=this.getComparablePrice(shopCart,descriptor)}ret.min=descriptor.promotion.min||0;ret.max=descriptor.promotion.max||value+1;this.shopCartCheckPromotionValue(value,ret);var langId=inditex.iLangId;if(ret.reached&&descriptor.reached.text&&descriptor.reached.text[langId])ret.text=descriptor.reached.text[langId],ret.clazz=descriptor.reached.clazz;else if(ret.left&&descriptor.left.text&&descriptor.left.text[langId]){var delta=parseFloat((descriptor.promotion.min-value).toFixed(2)),min=descriptor.promotion.min,
max=descriptor.promotion.max;"price"==descriptor.type&&(delta=inditex.formatPrice(parseInt(delta/currencyDecimals)),min=inditex.formatPrice(parseInt(min/currencyDecimals)),max=inditex.formatPrice(parseInt(max/currencyDecimals)));var toReplace=[delta,min,max];ret.text=descriptor.left.text[langId];ret.clazz=descriptor.left.clazz;i=0;for(len=toReplace.length;i<len;i++)ret.text=ret.text.replace("{"+i+"}",toReplace[i])}else ret.left=!1,ret.reached=!1}return ret};
bskPromotionsCtrl.prototype.transformPromotionsToShow=function(promotionsLeft,promotionsReached){var transformed=[];promotionsLeft.sort(function(promoA,promoB){return promoA.delta-promoB.delta});promotionsReached.sort(function(promoA,promoB){return promoA.min-promoB.min});for(var joined=promotionsReached.concat(promotionsLeft),i=0,len=joined.length;i<len;i++){var promotion=joined[i];transformed.push({text:promotion.text,clazz:promotion.clazz,type:promotion.left?"left":"reached"})}return transformed};
bskPromotionsCtrl.prototype.getPromotionsArray=function(promotions){var currentLeft=null,currentReached=null,promotionsToShowReached=[],promotionsToShowLeft=[],type;for(type in promotions)for(var activePromotions=promotions[type],i=0,len=activePromotions.length;i<len;i++){var activePromotion=activePromotions[i];if(activePromotion.left)if("left"==activePromotion.persistent||"all"==activePromotion.persistent)promotionsToShowLeft.push(activePromotion);else{if(!currentLeft||currentLeft.min>activePromotion.min)currentLeft=
activePromotion}else if(activePromotion.reached)if("reached"==activePromotion.persistent||"all"==activePromotion.persistent)promotionsToShowReached.push(activePromotion);else if(!currentReached||currentReached.min<activePromotion.min)currentReached=activePromotion}currentLeft&&promotionsToShowLeft.push(currentLeft);!currentReached||currentLeft&&"unique"==currentLeft.persistent||promotionsToShowReached.push(currentReached);return this.transformPromotionsToShow(promotionsToShowLeft,promotionsToShowReached)};
bskPromotionsCtrl.prototype.validatePromotionObj=function(descriptor){descriptor.promotion||(descriptor.promotion={});descriptor.location?$.isArray(descriptor.location)||(descriptor.location=descriptor.location.split(",")):descriptor.location=["all"];descriptor.exceptions||(descriptor.exceptions={});descriptor.exceptions.products||(descriptor.exceptions.products="");"string"==typeof descriptor.exceptions.products&&(descriptor.exceptions.products=descriptor.exceptions.products.split(","));!0===descriptor.persistent&&
(descriptor.persistent="all")};
bskPromotionsCtrl.prototype.getActivePromotions=function(){var self=this;this.shopCartService.loadShopCart(function(status,shopCart){if(1==status&&shopCart.items.length&&inditex.xActivePromotions){var countryPromotions=inditex.xActivePromotions[inditex.iCountryCode];if(!countryPromotions)return[];for(var currentPromotions={price:[],items:[],products:[]},i=0,len=countryPromotions.length;i<len;i++){var promotion=countryPromotions[i];self.validatePromotionObj(promotion);if(currentPromotions[promotion.type]){var check=
self.shopCartCheckPromotion(shopCart,promotion);(check.left||check.reached)&&currentPromotions[promotion.type].push(check)}}self.promotions=self.getPromotionsArray(currentPromotions)}else self.promotions=[]})};angular.module("itxApp").component("olapicSlider",{bindings:{apikey:"@",version:"@",items:"\x3d",selected:"\x3c"},templateUrl:"tplOlapicSlider",controller:["$q","olapicService","$element","trackingService",olapicSliderCtrl]});
function olapicSliderCtrl($q,olapicService,$element,trackingService){this.$q=$q;this.element=$element;this.trackingService=trackingService;this.olapicService=olapicService;this.streamWidth=160;this.maxStreamsShowed=2;this.report={}}olapicSliderCtrl.prototype.$onInit=function(){this.resetData()};olapicSliderCtrl.prototype.$onChanges=function(changes){changes.selected&&changes.selected.currentValue&&this._showPopup(changes.selected)};
olapicSliderCtrl.prototype.resetData=function(){this.selected=null;this.selectedIndex=this.olapicService.selected=null;this.showReportPanel=self.reportSended=!1};olapicSliderCtrl.prototype._removeViewedFlag=function(item){for(var i=item.streams.length-1;0<=i;i--)item.streams[i].viewed=!1};
olapicSliderCtrl.prototype._showPopup=function(selected){var self=this;this.showReportPanel=!1;this.selected=selected.currentValue;this.selectedIndex=this._getSelectedIndex();this.selected.streams?self._setStreamsValues():this.olapicService._getStreams(this.selected.id).then(function(data){self.selected.streams=data;self.items[self.selectedIndex].streams=data;self._setStreamsValues()},function(data){});this._setSocialLinks()};
olapicSliderCtrl.prototype._hidePopup=function(event){event&&event.preventDefault();this.resetData();$(document).triggerHandler("closePopUpEvent")};
olapicSliderCtrl.prototype.viewItem=function(num){var self=this;this.reportSended=this.showReportPanel=!1;if(null!=this.selectedIndex){var it=this._getItem(this.selectedIndex+num);it&&(this.selected=it.item,this.selectedIndex=it.index,this.showSocialLinks=!1,this.selected.streams?this._setStreamsValues():this.olapicService._getStreams(this.selected.id).then(function(data){self.selected.streams=data;self.items[self.selectedIndex].streams=data;self._setStreamsValues()},function(data){self._showSocialLinks();
self.trackPageView()}),this._setSocialLinks())}};olapicSliderCtrl.prototype._getSelectedIndex=function(){if(!this.items||!this.selected)return null;for(var i=0;i<this.items.length;i++){var item=this.items[i];if(item.id==this.selected.id)return i}return null};olapicSliderCtrl.prototype._getItem=function(index){if(!this.items&&!this.items.length)return null;index=0>index?this.items.length-1:index>=this.items.length?0:index;return{item:this.items[index],index:index}};
olapicSliderCtrl.prototype._setStreamsValues=function(){this._removeViewedFlag(this.selected);this.streamsWidth=this.selected.streams.length*this.streamWidth+"px";this.selected.streamPos=0;$(this.element).find(".olapicSli_stream_ul").css("left",0);this.showPrevBut=this._showPrevBut();this.showNextBut=this._showNextBut();this._showSocialLinks();this.trackPageView()};olapicSliderCtrl.prototype._showSocialLinks=function(){this.showSocialLinks=this.selected.streams&&this.selected.streams.length};
olapicSliderCtrl.prototype._showPrevBut=function(){return this.selected&&this.selected.streamPos&&0!=this.selected.streamPos};olapicSliderCtrl.prototype._showNextBut=function(){return this.selected&&this.selected.streams&&2<this.selected.streams.length?this.selected.streams.length-2>=this.selected.streamPos:!1};
olapicSliderCtrl.prototype.nextStreams=function(){this.selected.streamPos++;$(this.element).find(".olapicSli_stream_ul").animate({left:this.streamWidth*-this.selected.streamPos});this.trackImpressionEvent();this.showPrevBut=this._showPrevBut();this.showNextBut=this._showNextBut()};
olapicSliderCtrl.prototype.prevStreams=function(){this.selected.streamPos--;$(this.element).find(".olapicSli_stream_ul").animate({left:this.streamWidth*-this.selected.streamPos});this.showPrevBut=this._showPrevBut();this.showNextBut=this._showNextBut()};
olapicSliderCtrl.prototype._setSocialLinks=function(){var newUrlSocial=window.location.href,urlFacebook="http://www.facebook.com/sharer.php?u\x3d"+encodeURIComponent(newUrlSocial),urlTwitter="https://twitter.com/intent/tweet?text\x3d"+encodeURIComponent(this.selected.caption)+"\x26url\x3d"+encodeURIComponent(newUrlSocial),urlPinterest="https://www.pinterest.es/pin/create/button/?url\x3d\x26media\x3d"+this.selected.images.normal;this.socialLinks={facebook:urlFacebook,twitter:urlTwitter,pinterest:urlPinterest}};
olapicSliderCtrl.prototype.openSocial=function(social){var url="fb"==social?this.socialLinks.facebook:"tw"==social?this.socialLinks.twitter:"pt"==social?this.socialLinks.pinterest:null;url&&window.open(url,"_blank","height\x3d500,width\x3d600,status\x3d1,toolbar\x3d0,menubar\x3d0,location\x3d0,resizable\x3d1,scrollbars\x3d1")};olapicSliderCtrl.prototype.openReport=function(){this.reportSended=!1;this.showReportPanel=!0};
olapicSliderCtrl.prototype.closeReport=function(){this.showReportPanel=self.reportSended=!1};olapicSliderCtrl.prototype.sendReport=function(){var self=this;validateMultipleMails(this.report.email)&&this.olapicService.reportMedia({email:this.report.email,reason:this.report.reason},this.selected.id).then(function(data){self.reportSendedError=!1;self.reportSended=!0},function(){self.reportSended=!0;self.reportSendedError=!0})};
olapicSliderCtrl.prototype.trackPageView=function(){this.trackingService.trackPageviewSilent({name:"olapic-slider",data:{streams:this.selected.streams?this.selected.streams.slice(0,this.maxStreamsShowed):[]}});this._markStreamsAsViewed(this.selected.streamPos)};olapicSliderCtrl.prototype._markStreamsAsViewed=function(pos){this.selected.streams&&this.selected.streams.length&&(this._markStreamAsViewed(pos),this._markStreamAsViewed(++pos))};
olapicSliderCtrl.prototype._markStreamAsViewed=function(index){this.selected.streams[index]&&(this.selected.streams[index].viewed=!0)};olapicSliderCtrl.prototype.trackImpressionEvent=function(){var streams=this.selected.streams.slice(this.selected.streamPos,this.selected.streamPos+this.maxStreamsShowed).filter(function(stre){return 1!=stre.viewed});0!=streams.length&&(this.trackingService.trackEvent({name:"bershka-style-impression",data:{products:streams}}),this._markStreamsAsViewed(this.selected.streamPos))};
olapicSliderCtrl.prototype.gotoProduct=function(stream,pos){this.trackingService.trackEvent({name:"olapic-slider-product-click",data:{product:stream,position:pos+1}});location.href=stream.url};angular.module("itxApp").component("olapicCarousel",{bindings:{product:"\x3c?",showSeparator:"@?"},templateUrl:"tplOlapicCarousel",controller:["$q","$log","olapicService","$timeout","$element",olapicCtrl]});
function olapicCtrl($q,$log,olapicService,$timeout,$element){this.$q=$q;this.$log=$log;this.$timeout=$timeout;this.element=$element;this.olapicService=olapicService;this.uploaderSrc=bershka.config("OlapicUploadSrc");this.olapicLinkSrc=bershka.config("olapicLinkSrc");this.init.apply(this,arguments)}olapicCtrl.prototype.$onInit=function(){this._loadBindings()};
