ItxOrderDetailPageCtrl.prototype._updateOrder=function(order$$0){var self=this,action=this._getActionOrderDetail(order$$0);this.executeCache.execute({actions:[action],onFinish:function(status,order){if(1==status)for(var i=0;i<self.iScope.iGuestOrders.length;i++)if(self.iScope.iGuestOrders[i].id===order.id){self.iScope.iGuestOrders[i]=order;break}}},0)};
ItxOrderDetailPageCtrl.prototype._fetchGuestOrders=function(){var self=this,pageSize=bershka.config("GestOrdersPerPage");if(this.iScope.orderTokens&&this.iScope.orderTokens.orders&&this.iScope.orderTokens.orders.length)for(var i=0;i<pageSize&&0<this.iScope.orderTokens.orders.length;i++){var orderFor=this.iScope.orderTokens.orders.shift();if(orderFor.guestToken){var action$$0=this._getActionOrderDetail(orderFor);this.executeCache.execute({actions:[action$$0],onFinish:function(status,order){order.guestToken=
order.guestToken||orderFor.guestToken;order.autoLoginEnabled=self.iScope.autoLoginEnabled;order.items=self.returnHelpers.removeAutoAdded(order);if(1==status){"created"==order.eInvoiceStatus?self._getInvoiceAndPushOrder(order):self._pushOrder(order);var action=self.stateFactory.search("action");self.iScope.autoLoginEnabled&&("cancel"==action&&self.iScope.isCancelable(order)?self.iScope.showCancelConfirm(order):"return"==action&&self.iScope.isReturnable(order)&&self.iScope.setReturnOrder(order))}}},
0)}}};ItxOrderDetailPageCtrl.prototype._getActionOrderDetail=function(order){return{action:"order:order",params:{storeId:inditex.iStoreId,orderId:order.orderId||order.id,languageId:inditex.iLangId,catalogId:inditex.iCatalogId,token:order.guestToken}}};ItxOrderDetailPageCtrl.prototype.populateSuborderItem=function(order,item){for(var i=0;i<order.items.length;i++){var orderItem=order.items[i];if(item.id==orderItem.id)return orderItem}return item};
ItxOrderDetailPageCtrl.prototype.populateSubOrders=function(order){for(var i=0;i<order.suborders.length;i++){var suborder=order.suborders[i];if(suborder.items)for(var j=0;j<suborder.items.length;j++){var item=suborder.items[j];suborder.items[j]=this.populateSuborderItem(order,item)}}};
ItxOrderDetailPageCtrl.prototype._pushOrder=function(order){this._prepareOrderItems(order);var tempOrder=$.extend({},order);order.suborders?(order.suborders=order.suborders.slice(0),this.populateSubOrders(order)):order.suborders=[];0==order.suborders.length&&order.suborders.push(order);this.iScope.iGuestOrders.push(order);this.orderRepository.set(tempOrder);this.iScope.iGuestOrders.sort(function(orderA,orderB){return orderB.id-orderA.id})};
ItxOrderDetailPageCtrl.prototype._prepareOrderItems=function(order){for(var items=order.items,i=0,len=items.length;i<len;i++){var item=items[i];item.origin&&item.origin.length&&item.origin[0].style&&(item.style=item.origin[0].style)}};
