function(reason){onResourceLogout(reason)}))};HelpClient.addCallback=function(helpClientCallbackEnumValue,callback){callback||(callback=noop);callback=wrapCallback(callback);switch(helpClientCallbackEnumValue){case HelpClient.helpClientCallbackEnum.OnMessage:onMessage=callback;break;case HelpClient.helpClientCallbackEnum.OnResourceLogout:onResourceLogout=callback;break;case HelpClient.helpClientCallbackEnum.OnReceipt:onReceipt=callback;break;case HelpClient.helpClientCallbackEnum.OnClose:onClose=
callback;break;case HelpClient.helpClientCallbackEnum.OnDisconnect:onDisconnect=function(error){currentWorkgroupJid=null;callback(error)};break;case HelpClient.helpClientCallbackEnum.OnTypingOn:onTypingOn=callback;break;case HelpClient.helpClientCallbackEnum.OnTypingOff:onTypingOff=callback;break;case HelpClient.helpClientCallbackEnum.OnReconnecting:onReconnecting=callback;break;case HelpClient.helpClientCallbackEnum.OnReconnected:onReconnected=callback}};HelpClient.joinRoom=function(roomJid){client.joinRoom(roomJid)};
HelpClient.login=function(contact,workgroupJid,onSuccess,onError,retries){onSuccess=onSuccess||noop;onError=onError||noop;currentWorkgroupJid=workgroupJid;var shouldJoin=!contact.jid;contact.jid=Mochat.normalizeJid(contact.jid);client.connect(contact.jid,contact.wcsuid,"all",null,function onConnect(){var metadata={};metadata.contactJid=contact.jid;metadata.contactWcsuid=contact.wcsuid;metadata.contactName=contact.name;metadata.contactEmail=contact.email;metadata.contactOrderId=contact.orderId;metadata.contactSku=
contact.sku;metadata.contactDevice=contact.device;metadata.contactPage=contact.page;metadata.contactTopic=contact.topic;metadata.contactCountryCode=contact.countryCode;metadata.isMediaDeriverableToCustomer=contact.isMediaDeriverableToCustomer;shouldJoin?client.joinWorkgroup(currentWorkgroupJid,metadata,function onJoin(){onSuccess(contact.jid)},function onForcedLeave(){HelpClient.logout();onClose()},function onInternalError(error,stanza){onError(Mochat.Datatypes.Error.unknownError(error.message))}):
(client.setWorkgroupInServer(currentWorkgroupJid),client.setupWorkgroupHandlers(currentWorkgroupJid,"all",function onForcedLeave(){HelpClient.logout();onClose()}),onSuccess(contact.jid))},function onInternalDisconnect(error){onDisconnect(Mochat.Datatypes.Error.unknownError(error.message))},function onInternalError(error,stanza){onError(Mochat.Datatypes.Error.unknownError(error.message))},retries,function onInternalReconnecting(){onReconnecting()},function onInternalReconnected(){onReconnected()});
return contact.jid};HelpClient.logout=function(){client&&client.leaveWorkgroup(currentWorkgroupJid,function success(){client.disconnect("logout",!0);currentWorkgroupJid=null},function err(){client.disconnect("logout",!0);currentWorkgroupJid=null})};HelpClient.message=function(body,metadata,attachment,messageId,onSlotRequestSuccess,onSlotRequestError){var maskedBody=Mochat.maskCreditCards(body);client.send(currentWorkgroupJid,Nomasystems.ChatCore.Model.Message.Type.Workgroup,Nomasystems.ChatCore.Model.Message.State.Active,
