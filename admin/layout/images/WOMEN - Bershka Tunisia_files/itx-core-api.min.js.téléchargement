id=this.iScope.addressForm.shipping&&this.iScope.addressForm.shipping.id);for(var j=0,len2=addresses.length;j<len2;j++)if(addresses[j]==id)return!0;break}}return!1};ItxOrderRepaymentPageClass.prototype.getActivePaymentForm=function(){return $(".payment-form."+this.iScope.paymentSelected.kind+"-form")};ItxOrderRepaymentPageClass.prototype.normalizeAddress=function(address){!inditex.hasLastName()&&address&&address.lastName&&(address.lastName="");return address};
