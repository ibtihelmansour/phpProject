shopCartItemCtrl.prototype.getProductImage=function(){if(this.isGiftWrap)return inditex.getStaticResourceUrl("images/regalo/envoltorio_regalo.jpg");var image=this.iEditMode?this.selectedColor.originalColor.image:this.iProduct.image,stylismId;stylismId=image&&image.style&&image.style.length?image.style[0]:this.iProduct.origin&&this.iProduct.origin.length?this.iProduct.origin[0].style:this.iProduct.style;var colorId=this.iEditMode?this.selectedColor.id:this.iProduct.colorId,detailImage=this.imageService.getProductDetailImages(this.iProduct,
